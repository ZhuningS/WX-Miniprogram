    src: url('//at.alicdn.com/t/font_521473_c9tysafwoob6gvi.eot?t=1514260098519#iefix') format('embedded-opentype'),url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),url('//at.alicdn.com/t/font_521473_c9tysafwoob6gvi.ttf?t=1514260098519') format('truetype'),url('//at.alicdn.com/t/font_521473_c9tysafwoob6gvi.svg?t=1514260098519#iconfont') format('svg');
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-xiaofeijifen:before {
    content: "\e6eb";
}

.icon-jifen:before {
    content: "\e6ec";
}

.icon-huiyuan:before {
    content: "\e6ed";
}

.icon-huiyuandengji:before {
    content: "\e6ee";
}

.icon-jiaoyijilu:before {
    content: "\e6ef";
}

.icon-jifenduihuan:before {
    content: "\e6f0";
}

.icon-zhuanxiangzhekou:before {
    content: "\e6f1";
}

.iconfont {
    font-size: inherit;
    display: inline-block;
    vertical-align: bottom;
}

page {
    font-size: 32rpx;
}

.fb {
    font-weight: bold;
}

.member-card-box {
    padding: 30rpx 30rpx 0;
    background: #3c3e43;
}

.member-card {
    padding: 30rpx;
    background-image: -webkit-linear-gradient(-4deg,rgb(201,176,132) 0%,rgb(212,188,145) 100%);
    color: #fff;
    border-radius: 16rpx 16rpx 0 0;
}

.user-info {
    align-items: center;
    line-height: 100%;
}

.user-img {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    box-shadow: 0 0 0 2px rgba(255,255,255,0.4);
}

.member-grade {
    font-size: 28rpx;
    margin-top: 16rpx;
}

.write-code {
    font-size: 50rpx;
}

.card-money {
    margin-top: 40rpx;
    align-items: center;
}

.balance-tit {
    opacity: .6;
}

.card-recharge {
    background: transparent;
    border: 1px solid;
    color: #fff;
    border-radius: 60rpx;
    line-height: 50rpx;
    font-size: 28rpx;
}

.card-recharge::after {
    border: none;
}

.card-recharge.button-hover {
    background: rgba(255,255,255,0.2);
}

.card-balance {
    font-size: 36rpx;
    line-height: 1.3;
}

.card-balance-money {
    font-size: 50rpx;
}

.vip-number {
    position: relative;
    margin: 0 -30rpx -30rpx -30rpx;
    padding: 20rpx 30rpx 10rpx;
    background: linear-gradient(rgba(255,255,255,0),rgba(0,0,0,0.2));
    color: rgba(255,255,255,0.8);
    letter-spacing: 1px;
    text-shadow: 1px 1px 2px rgba(0,0,0,0.4);
}

.recharge-pop {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 2;
}

.recharge-popbg {
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.recharge-cont {
    width: 650rpx;
    border-radius: 12rpx;
    background: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    padding: 60rpx 70rpx;
    z-index: 1;
    text-align: center;
    overflow: hidden;
    box-sizing: border-box;
}

.recharge-colse {
    position: absolute;
    right: 0;
    top: 0;
    background: #c9b084;
    width: 78rpx;
    height: 78rpx;
    border-bottom-left-radius: 80%;
}

.recharge-colse::before,.recharge-colse::after {
    content: '';
    display: block;
    width: 50%;
    height: 1px;
    background: #fff;
    position: absolute;
    right: 20%;
    top: 45%;
    transform: rotate(45deg);
}

.recharge-colse::after {
    transform: rotate(-45deg);
}

.recharge-colse:activeP {
    opacity: 0.8;
}

.recharge-btn,.recharge-inp {
    background: transparent;
    margin-top: 30rpx;
    color: #c9b084;
    border: 1px solid;
    line-height: 72rpx;
    font-size: 30rpx;
    border-radius: 6rpx;
}

.recharge-btn.active,.recharge-inp.active {
    background: rgba(201,176,132,0.2);
}

.recharge-btn::after {
    border: none;
}

.recharge-inp {
    display: flex;
    padding: 0 20rpx;
}

.recharge-inp-txt {
    flex: 1;
    height: 72rpx;
    margin: 0 20rpx;
}

.recharge-sure {
    margin-top: 50rpx;
    background: #c9b084;
    color: #fff;
    border-radius: 6rpx;
    font-size: 32rpx;
    line-height: 86rpx;
}

.recharge-item {
    max-height: 400rpx;
    overflow: hidden;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
}

.card-integral {
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #4d5057;
    padding: 30rpx;
    color: #fff;
}

.card-integral .iconfont {
    font-size: 1.2em;
}

.card-desc-tit {
    padding: 30rpx;
    background: #fff;
}

.card-desc {
    background: #fff;
}

.card-desc-item {
    padding: 20rpx 30rpx;
    display: flex;
}

.card-desc-item .iconfont {
    font-size: 40rpx;
}

.card-desc-cont {
    flex: 1;
    margin-left: 30rpx;
}

.desc-item-tit {
    font-weight: bold;
}