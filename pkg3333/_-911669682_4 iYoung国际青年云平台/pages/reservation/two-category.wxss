.table-view-cell {
    position: relative;
    padding: 20rpx 30rpx;
    word-break: break-all;
    overflow: hidden;
}

.media-object {
    width: 128rpx;
    height: 128rpx;
}

.table-view-cell .title {
    display: block;
    font-weight: bold;
}

.choose {
    line-height: 128rpx;
}

.num-btn {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
}

.num-btn:active {
    background: rgba(0,0,0,0.05);
}

.yuyue-num {
    display: inline-block;
    width: 50rpx;
    text-align: center;
}

.h1 {
    display: block;
    font-size: 32px;
    line-height: 1.2;
    font-weight: bold;
    margin: 25px auto;
}

.empty {
    margin-top: 25%;
    text-align: center;
    color: rgba(0,0,0,0.3);
}

.btn-box {
    height: 92rpx;
}

.btn {
    position: fixed;
    bottom: 0;
    width: 100%;
}

.btn .totle-price {
    padding: 10rpx 0;
    padding-left: 30rpx;
    line-height: 1.4;
}

.btn button {
    border-radius: 0;
    min-width: 180rpx;
}

.btn button::after {
    border: none;
}

@import "../../__wuBaseWxss__/2.wxss";