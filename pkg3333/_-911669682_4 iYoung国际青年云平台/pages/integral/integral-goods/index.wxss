@font-face {
    font-family: "iconfont";
    src: url('//at.alicdn.com/t/font_500896_ieqge8ynpn0l766r.eot?t=1512629257067');
    src: url('//at.alicdn.com/t/font_500896_ieqge8ynpn0l766r.eot?t=1512629257067#iefix') format('embedded-opentype'),url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),url('//at.alicdn.com/t/font_500896_ieqge8ynpn0l766r.ttf?t=1512629257067') format('truetype'),url('//at.alicdn.com/t/font_500896_ieqge8ynpn0l766r.svg?t=1512629257067#iconfont') format('svg');
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-time:before {
    content: "\e65f";
}

.icon-shuoming:before {
    content: "\e7c2";
}

@import "../../../wxParse/wxParse.wxss";

.iconfont {
    font-size: inherit;
}

page {
    font-size: 32rpx;
}

.red {
    color: #ee4a4a;
}

.integral-goods-img {
    width: 100%;
    padding-top: 50%;
    position: relative;
}

.integral-goods-img .swiper-goods-img {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.integral-goods-img image {
    width: 100%;
    height: 100%;
}

.goods-msg {
    background: #fff;
    padding: 30rpx;
}

.goods-name {
    font-size: 34rpx;
}

.integral-num {
    line-height: 1.3;
    margin-top: 20rpx;
}

.exchange-time {
    padding: 20rpx 30rpx;
    background: #fbfbfb;
    color: #666;
    font-size: 28rpx;
}

.exchange-note {
    background: #fff;
    padding: 30rpx;
}

.exchange-note-tit {
    color: #ebc35c;
}

.exchange-note image {
    max-width: 100%!important;
}

.exchange-note view {
    white-space: pre-wrap;
    max-width: 100%!important;
}

.fiexd-box {
    height: 92rpx;
}

.integral-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
}

.integral-bottom button {
    border-radius: 0;
}

.integral-bottom button::after {
    border: none;
}

.integral-btn {
    background: #ebc35c;
    color: #fff;
}

.integral-btn:active {
    background: #e2bb55;
}

.redeem-code {
    background: #666;
    color: #fff;
}

.integral-pop {
    z-index: 10;
}

.integral-pop .floatBoxBg {
    display: block;
}

.integral-cont {
    background: #fff;
    width: 500rpx;
    border-radius: 10rpx;
    overflow: hidden;
    z-index: 1;
}

.integral-cont-tit {
    padding: 40rpx 30rpx 20rpx;
    text-align: center;
    font-weight: 700;
}

.integral-cont button {
    border-radius: 0;
    line-height: 86rpx;
    height: 86rpx;
    font-size: 32rpx;
}

.integral-cont button::after {
    border: none;
}

.cancel-btn {
    background: #fafafa;
}

.cancel-btn:active {
    background: #f8f8f8;
}

.confirm-btn {
    background: #ebc35c;
    color: #fff;
}

.confirm-btn:active {
    background: #e2bb55;
}

.selTel {
    padding: 20rpx 30rpx;
    background-color: rgba(245,245,245,1);
}

.cell {
    padding: 0 30rpx;
    margin-bottom: 20rpx;
}