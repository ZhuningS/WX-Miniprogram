.list {
    padding: 30rpx;
    background: #fff;
}

.list .title {
    position: relative;
    font-weight: bold;
}

.list-img {
    display: inline-block;
    width: 320rpx;
    height: 160rpx;
    margin-top: 20rpx;
}

.list-img:nth-child(2n+2) {
    margin-left: 20rpx;
}

.list-img image {
    width: 100%;
    height: 100%;
    border-radius: 6rpx;
}