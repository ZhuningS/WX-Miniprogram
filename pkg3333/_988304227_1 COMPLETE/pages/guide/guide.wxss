swiper {
    width: 100%;
    height: 100vh;
}

swiper .bg {
    width: 100%;
    height: 100%;
}

.bird {
    position: absolute;
    width: 265rpx;
    right: 0;
}

.master {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 600rpx;
    height: 600rpx;
}

.text {
    position: absolute;
    width: 100%;
    text-align: center;
}

.text .title {
    font-size: 48rpx;
    font-weight: 700;
    color: #22442A;
    margin-bottom: 30rpx;
}

.text text {
    color: #22442A;
    font-size: 32rpx;
}

swiper .go {
    width: 250rpx;
    height: 60rpx;
    border: 0;
    line-height: 60rpx;
    background: #689864;
    font-size: 30rpx;
    color: #fff;
    border-bottom: 2rpx solid #447A45;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
}