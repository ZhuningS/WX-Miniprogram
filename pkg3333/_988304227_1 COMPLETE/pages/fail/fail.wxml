<view id="header">
    <view style="height:{{headerHeight}}rpx;"></view>
    <view class="capsule">
        <view bindtap="reportTee" class="seat"></view>
        <image mode="widthFix" src="../../assets/images/arr_left_black.png"></image>
    </view>
</view>
<view class="container">
    <image class="title" mode="widthFix" src="../../assets/images/font_three@3x.png" style="margin-top: {{top+164}}rpx"></image>
    <image class="bird" mode="widthFix" src="../../assets/images/img_bird@3x.png" style="top: {{top+90}}rpx;"></image>
    <image class="tee" mode="widthFix" src="../../assets/images/dead tree_big@3x.png"></image>
    <button bindtap="reportTee" hoverClass="none" plain="true">再次种树</button>
</view>
