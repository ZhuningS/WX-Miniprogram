slider {
    width: 550rpx;
    margin: 10rpx auto 90rpx auto;
}

.container {
    width: 100%;
    height: 100vh;
    text-align: center;
    position: relative;
}

.showTime {
    font-size: 48rpx;
    color: #fff;
    margin-top: 40rpx;
}

.bottomBar {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 166rpx;
    background: rgb(238,251,232);
    display: flex;
    justify-content: space-between;
    padding: 0 70rpx;
    box-sizing: border-box;
    align-items: center;
}

.bottomBar image {
    width: 60rpx;
    height: 60rpx;
}

.bottomBar view {
    text-align: center;
    font-size: 28rpx;
    color: #22442A;
    margin-top: 10rpx;
}

.title {
    height: 55rpx;
}

.title image {
    width: 240rpx;
}

.tee {
    width: 300rpx;
}

.container .start {
    width: 250rpx;
    height: 60rpx;
    background: #689864;
    line-height: 60rpx;
    border: 0;
    font-size: 30rpx;
    border-bottom: 2rpx solid #447A45;
    color: #fff;
}

.bird {
    width: 265rpx;
    position: absolute;
    right: 0;
    top: 208rpx;
}

.teeContainer {
    width: 100%;
    height: 450rpx;
    margin: 110rpx auto 0 auto;
    text-align: center;
}

.countDown {
    font-size: 100rpx;
    color: #fff;
    margin-bottom: 50rpx;
}

.mask {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(14,41,21,0.5);
    z-index: 100;
}

.model {
    width: 550rpx;
    height: 437rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background: #fff;
    border-radius: 10rpx;
    color: #666666;
    text-align: center;
    font-size: 27rpx;
}

.model .title {
    width: 100%;
    margin: 70rpx 0 29rpx 0;
    color: #333333;
    font-size: 32rpx;
}

.model button {
    border: 0;
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #999;
    font-size: 32rpx;
    border-radius: 4rpx;
}

.model .giveUp {
    margin-top: 70rpx;
    background: #5B8C30;
    color: #fff;
}