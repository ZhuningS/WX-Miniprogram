.container {
    width: 100%;
    height: 100vh;
    text-align: center;
    position: relative;
    background: url(data:image/jpeg;base64,) repeat-y;
}

#header {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    z-index: 1000;
}

#header .capsule {
    height: 88rpx;
    line-height: 88rpx;
    position: relative;
    text-align: center;
}

#header .capsule text {
    font-size: 34rpx;
    color: #333333;
}

#header .capsule image {
    width: 23rpx;
    position: absolute;
    left: 24rpx;
    top: 50%;
    transform: translateY(-50%);
}

#header .seat {
    position: absolute;
    width: 200rpx;
    height: 88rpx;
    left: 0;
    top: 0;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}