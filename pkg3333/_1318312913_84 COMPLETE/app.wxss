.bar {
    height: 60px;
    display: flex;
    align-items: center;
    padding: 0 60rpx;
    width: 100%;
    box-sizing: border-box;
    background: #FFFFFF;
}

.bar .button.action {
    margin: 10px 10px 10px -20px;
}

.bar .button {
    background: initial;
    color: initial;
    margin: 0 -30rpx 0 10rpx;
}

.input-wrapper {
    flex: 1;
    display: flex;
    align-items: center;
    background: rgba(0,0,0,0.05);
    height: 40px;
}

.input-wrapper .delete {
    opacity: 0.1;
}

