    font-weight: 300;
    font-size: 16px;
    line-height: 25px;
    max-height: 25px;
}

.description,.qm-tip .qm-tip-ctn {
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 14px;
    line-height: 20px;
    color: black;
    max-height: 40px;
}

.description.white,.qm-tip .white.qm-tip-ctn {
    color: white;
}

.meta,.caption,.qm-avatars,.qm-avatar.more {
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 12px;
    line-height: 15px;
    color: rgba(0,0,0,0.2);
