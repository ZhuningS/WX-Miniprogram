    font-weight: bold;
    font-size: 48px;
    line-height: 60px;
    max-height: 120px;
    letter-spacing: -0.06em;
}

.headline1 {
    overflow: hidden;
    font-weight: bold;
    font-size: 18px;
    line-height: 25px;
    max-height: 50px;
}

.headline2 {
    overflow: hidden;
    text-overflow: ellipsis;
    font-weight: bold;
    font-size: 24px;
    line-height: 30px;
    max-height: 60px;
}

.label1,.page-name .title {
    overflow: hidden;
