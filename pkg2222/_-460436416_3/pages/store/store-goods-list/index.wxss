    -webkit-box-align: center;
}

.ress-main {
    width: 100%;
    height: 100vh;
    box-orient: vertical;
}

.ress-nav {
    background: #fff;
    line-height: 92rpx;
    height: 92rpx;
    align-items: center;
}

.ress-nav-item {
    position: relative;
    height: 92rpx;
    color: #666;
    transition: all 0.3s linear;
}

.ress-nav-item::before {
    content: "";
    display: block;
    width: 60%;
    height: 2px;
    background: var(--DStore-mcolor);
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    opacity: 0;
}

.ress-nav-item.active {
    font-weight: bold;
    color: var(--DStore-mcolor);
}

.ress-nav-item.active::before {
    width: 20%;
    opacity: 1;
}

.shop-home-btn {
    background: var(--DStore-mcolor);
    color: #fff;
    font-size: 28rpx;
    border-radius: 6rpx;
    line-height: 62rpx;
    height: 62rpx;
    padding: 0 20rpx;
    margin: 0 30rpx;
}

.ress-post {
    height: 92rpx;
    background: #fff;
}

.ress-post-item {
    padding: 0 30rpx;
    height: 92rpx;
