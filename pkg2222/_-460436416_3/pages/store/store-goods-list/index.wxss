}

.ress-side-item {
    overflow: hidden;
    white-space: nowrap;
    color: #888;
    text-overflow: ellipsis;
}

.ress-side-active {
    position: relative;
    color: var(--DStore-mcolor);
    font-weight: bold;
}

.ress-side-active::after {
    content: '';
    display: block;
    width: 6rpx;
    height: 100%;
    background: var(--DStore-mcolor);
    position: absolute;
    left: 0;
    top: 0;
}

.ress-con {
    flex: 1;
