}

.rich-text {
    white-space: pre-wrap;
}

.red-packet-tag {
    background: #f66;
    color: #fff;
    display: inline-block;
    padding: 0 16rpx;
    line-height: 20px;
    font-size: 10px;
    border-radius: 6rpx;
    margin-right: 20rpx;
}

.aside-more {
    padding: 30rpx;
    position: relative;
}

.aside-more.arrowB::before {
    transform: translateY(-88%) scale(1.1) rotate(135deg);
}

.more-content {
    position: absolute;
    top: 80rpx;
    right: 20rpx;
    line-height: 88rpx;
    z-index: 3;
    width: inherit;
    border-radius: 2px;
    box-shadow: 0 8rpx 12rpx rgba(0,0,0,0.1);
    animation: showMore .2s .1s ease forwards;
    transform: translateY(20px);
    opacity: 0;
}

.more-content::before {
    content: '';
    border-bottom: 20rpx solid #fff;
    border-left: 20rpx solid transparent;
    border-right: 20rpx solid transparent;
    position: absolute;
    top: -18rpx;
    right: 22rpx;
}

.more-content .item {
    padding: 0 40rpx;
    min-width: 200rpx;
}

.more-content .item:last-child::before {
    display: none;
}

.more-content .item:active {
    background: rgba(0,0,0,0.05);
}

.floatBox.active {
    z-index: 1;
}

.floatBox .floatBoxBg {
    display: none;
    opacity: 0;
    animation: blackBg 0.3s ease forwards;
}

.floatBox.active .floatBoxBg {
    display: block;
}

@-webkit-keyframes blackBg {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@keyframes blackBg {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@-webkit-keyframes showMore {
    0% {
        -webkit-transform: translateY(20px);
        opacity: 0;
    }

    100% {
        -webkit-transform: translateY(0);
        opacity: 1;
    }
}

@keyframes showMore {
    0% {
        transform: translateY(20px);
        opacity: 0;
    }

    100% {
        transform: translateY(0);
        opacity: 1;
    }
}

.zanactive {
    animation: zan .8s forwards;
}

@-webkit-keyframes zan {
    0% {
        transform: scale(1);
        opacity: 1;
    }

    50% {
        transform: scale(1.4) rotate(-45deg);
        opacity: .8;
    }

    100% {
        transform: scale(1);
        opacity: 1;
    }
}

@keyframes zan {
    0% {
        transform: scale(1);
        opacity: 1;
    }

    50% {
        transform: scale(1.4) rotate(-45deg);
        opacity: .8;
    }

    100% {
        transform: scale(1);
        opacity: 1;
    }
}

.recomme {
