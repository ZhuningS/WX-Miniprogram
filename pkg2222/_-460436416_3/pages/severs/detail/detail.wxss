.iconfont {
    font-size: inherit;
    color: inherit;
}

.gridFour::before {
    border-color: currentColor;
}

.icon-appreciatefill::before {
    color: inherit;
}

.bgwhite {
    background: #fff;
}

.red {
    color: #f66;
}

.floatBox {
    z-index: 2;
}

.top {
    padding: 20rpx 30rpx;
}

.content {
    word-break: break-all;
    white-space: normal;
