.aside-float {
    position: fixed;
    bottom: 120rpx;
    right: 30rpx;
    z-index: 0;
}

.aside-float button {
    width: 76rpx;
    height: 76rpx;
    line-height: 76rpx;
    border-radius: 50%;
    padding: 0;
    font-size: 36rpx;
}

.aside-float button:nth-child(n+2) {
    margin-top: 10rpx;
}

.red-bag-float {
    position: fixed;
    bottom: 280rpx;
    right: 0;
    width: 240rpx;
    height: 240rpx;
    overflow: hidden;
}

.red-bag-float image {
    width: 160rpx;
    height: 160rpx;
    margin: 40rpx;
    animation: redBag .5s 0.5s ease forwards,redBag2 .35s 1s ease infinite;
    transform: translate(0%,-200%);
    position: relative;
    z-index: 1;
    display: block;
    transform-origin: center bottom;
}

.gold-coin {
    display: block;
    width: 60rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    color: rgba(255,87,34,0.8);
    font-size: 38rpx;
    font-weight: bold;
    border-radius: 50%;
    background: #fecb0f;
    border: 3px #ffe26a;
    position: absolute;
    top: 0;
    right: 0;
    transform: translate(0%,-100%) scale(0.3);
    animation: goldIcon 2s 0.8s ease infinite;
}

@-webkit-keyframes goldIcon {
    0% {
        transform: translate(0%,-100%) scale(0.3);
    }

    50%,100% {
        transform: translate(-300%,400%) scale(0.5);
    }
}

@keyframes goldIcon {
    0% {
        transform: translate(0%,-100%) scale(0.3);
    }

    50%,100% {
        transform: translate(-300%,400%) scale(0.5);
    }
}

.gold-2 {
    right: 20%;
    animation-delay: 1.2s;
}

.gold-3 {
    right: 50%;
    animation-delay: 1.5s;
    z-index: 2;
}

.gold-4 {
    right: 90%;
    animation-delay: 1.7s;
    z-index: 2;
}

.gold-5 {
    right: -20%;
    animation-delay: 2s;
    z-index: 2;
}

.gold-6 {
    right: 60%;
    animation-delay: 2.5s;
    z-index: 2;
}

.gold-7 {
    right: 30%;
    animation-delay: 2.3s;
    z-index: 2;
}

@-webkit-keyframes redBag {
    0% {
        transform: translate(0%,-200%);
    }

    33% {
        transform: translate(0%,-20%);
    }

    66% {
        transform: translate(0%,10%);
    }

    100% {
        transform: translate(0%,0%);
    }
}

@keyframes redBag {
    0% {
        transform: translate(0%,-200%);
    }

    33% {
        transform: translate(0%,-20%);
    }

    66% {
        transform: translate(0%,10%);
    }

    100% {
        transform: translate(0%,0%);
    }
}

@-webkit-keyframes redBag2 {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1,.92);
    }

    100% {
        transform: scale(1,1);
    }
}

@keyframes redBag2 {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1,.92);
    }

    100% {
        transform: scale(1,1);
    }
}

.red-packets-box .floatBoxBg {
    display: block;
}

.red-packets-box {
    display: flex;
    align-items: center;
    justify-content: center;
}

.red-packets {
    width: 560rpx;
    height: 656rpx;
    background: #ff4b4b;
    color: #ffd33b;
    z-index: 3;
    border-radius: 12rpx;
    padding-top: 80rpx;
    overflow: hidden;
    position: relative;
}

.red-packets-close {
    width: 66rpx;
    height: 66rpx;
    color: rgba(0,0,0,0.3);
    position: absolute;
    top: 0;
    right: 0;
}

.red-packets-close:active {
    background: rgba(0,0,0,0.05);
}

.red-packets-close::before,.red-packets-close::after {
    content: '';
    display: block;
    width: 50%;
    height: 2px;
    background: currentColor;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%) rotate(45deg);
}

.red-packets-close::after {
    transform: translate(-50%,-50%) rotate(-45deg);
}

.packets-tips {
    opacity: 0.5;
}

.packets-money {
    font-size: 80rpx;
    line-height: 1.8;
}

.packets-from {
    color: #fff;
    opacity: 0.6;
    padding: 0 40rpx 20rpx;
    margin: 0 auto;
}

.red-packets-bottom {
    width: 100%;
    background: #ee443d;
    min-height: 100rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    margin-top: 200rpx;
}

.red-packets-bottom::before {
    content: '';
    display: block;
    width: 106%;
    height: 200rpx;
    border-radius: 50% 50% 0 0;
    background: #ee443d;
    position: absolute;
    top: 0;
    margin-left: -3%;
    margin-top: -100rpx;
    box-shadow: 0 0 6px rgba(161,25,20,0.6);
    z-index: -1;
}

.open-packets {
    width: 140rpx;
    height: 140rpx;
    line-height: 140rpx;
    font-size: 48rpx;
    color: #d18300;
    background: #f8c156;
    border-radius: 50%;
    position: absolute;
    top: -170rpx;
    left: calc(50% - 70rpx);
    box-shadow: 0 2px 7px rgba(161,25,20,0.63);
}

.open-packets image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.red-packets .more-details {
    width: 100%;
    padding: 20rpx 0;
    opacity: 0.5;
}

.red-packets .more-details::after {
    content: '';
    display: inline-block;
    width: 6px;
    height: 6px;
    border-top: 1px solid;
    border-right: 1px solid;
    transform: rotate(45deg);
    transform-origin: center;
    margin-left: 6rpx;
}

.word-inp {
    background: rgba(255,255,255,0.6);
    width: 70%;
    height: 68rpx;
    border-radius: 60rpx;
    margin: 20rpx auto;
    padding: 0 20rpx;
    color: #ee443d;
}

.word-inp input {
    height: 68rpx;
    font-size: 30rpx;
}

.word-plchdcolor {
    color: #f75258;
    font-size: 30rpx;
}

.word-inp-prompt {
    color: #fff;
    opacity: 0.8;
    animation: buzz-out 0.2s 0.1s linear forwards;
    animation-iteration-count: 3;
}

@-webkit-keyframes buzz-out {
    0% {
        transform: translateX(0);
    }

    50% {
        transform: translateX(-5px);
    }

    50% {
        transform: translateX(5px);
    }

    100% {
        transform: translateX(0);
    }
}

@keyframes buzz-out {
    0% {
        transform: translateX(0);
    }

    50% {
        transform: translateX(-5px);
    }

    50% {
        transform: translateX(5px);
    }

    100% {
        transform: translateX(0);
    }
}

.open-packets.active {
    animation: packetsRotate 0.6s 0.1s infinite;
    -webkit-cubic-bezier: (1,-0.39,.39,1.44);
    transform-origin: center;
}

@-webkit-keyframes packetsRotate {
    0% {
        transform: rotateY(0);
    }

    100% {
        transform: rotateY(360deg);
    }
}

@keyframes packetsRotate {
    0% {
        transform: rotateY(0);
    }

    100% {
        transform: rotateY(360deg);
    }
}

.red-packets.active {
    animation: redPacketsActive 0.3s ease forwards;
    border-radius: 0;
    padding-top: 0;
}

.red-packets.active .red-inner-box {
    height: 100%;
}

.red-packets.active .red-packets-bottom {
    background: #fff;
    position: absolute;
    top: 144rpx;
}

.red-packets.active .red-packets-bottom::before {
    background: #fff;
}

.red-packets.active .open-packets {
    box-shadow: none;
    transition: all 0.3s 0.1s ease;
}

.red-packets.active .packets-from {
    color: #333;
    font-size: 32rpx;
    background: #fff;
}

.red-packets.active .packets-details-box {
    text-align: left;
    background: #fff;
    position: absolute;
    left: 0;
    top: 60rpx;
    padding: 60rpx 0 30rpx;
    right: 0;
    bottom: 0;
}

.pac-item-tit {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    padding: 0 30rpx;
    line-height: 60rpx;
}

.pac-item-box {
    height: 100%;
    padding: 0 30rpx;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
}

.pac-item {
    padding: 10rpx 0;
    color: #333;
}

.pac-user-img {
    width: 80rpx;
    height: 80rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.pac-user-img image {
    width: 100%;
    height: 100%;
}

@-webkit-keyframes fadeIn {
    100% {
        transform: scaleY(1);
    }
}

@keyframes fadeIn {
    100% {
        transform: scaleY(1);
    }
}

.red-packets.active .detail-tips-active {
    overflow: hidden;
    display: none;
}

@-webkit-keyframes redPacketsActive {
    100% {
        width: 100%;
        height: 100%;
    }
}

@keyframes redPacketsActive {
    100% {
        width: 100%;
        height: 100%;
    }
}

