@import "../../../__wuBaseWxss__/13.wxss";

@import "../../../__wuBaseWxss__/2.wxss";

.topBox {
    padding: 30rpx;
}

.logoPic {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
}

.addr {
    line-height: 44rpx;
}

.infoBox {
    font-size: 30rpx;
    line-height: 56rpx;
}

.infoBox .mr15 {
    color: #666;
}

.descBox {
    padding: 30rpx;
}

.descCon {
    color: #666;
    font-size: 24rpx;
    line-height: 40rpx;
}

.descPic {
    width: 100%;
    height: 345rpx;
}

.uploadBtn {
    background-color: #fff;
    font-size: 32rpx;
    line-height: 60rpx;
    justify-content: center;
    align-items: center;
}

.uploadBtn::after {
    border: none;
}

.uploadBtn:active {
    background-color: rgba(0,0,0,0.03);
}

.plus {
    width: 32rpx;
    height: 32rpx;
    text-align: center;
    line-height: 32rpx;
    border: 1rpx solid currentColor;
    border-radius: 50%;
    margin-right: 12rpx;
}

.footer {
    position: fixed;
    bottom: 0;
    z-index: 10;
    width: 100%;
    height: 92rpx;
}

.footLf {
    color: #666;
}

.footLf .sub:active {
    background-color: rgba(0,0,0,0.1);
}

.shareBtn {
    height: 92rpx;
    line-height: 50rpx;
    font-size: 24rpx;
    background-color: #fff;
    border-radius: 0;
}

.shareBtn::after {
    border: 0;
}

.footLf .iconfont {
    font-size: 36rpx;
}

.iconTit {
    margin-top: -12rpx;
}

.telBtn {
    color: #fff;
    width: 340rpx;
    border-radius: 0;
    line-height: 92rpx;
}

.telBtn::after {
    width: 0;
    height: 0;
}

.telBtn:active {
    opacity: 0.85;
}

.itemTop {
    padding: 30rpx;
}

.itemTop .shInfo {
    color: #666;
    line-height: 56rpx;
    font-size: 28rpx;
}

.itemBot {
    height: 120rpx;
    text-align: right;
    padding: 30rpx;
    margin-top: 0;
}

.itemBot .btn {
    display: inline-block;
    margin: 0 15rpx;
    line-height: 60rpx;
    font-size: 28rpx;
}

.itemBot .btn::after {
    border-color: currentColor;
}

.concat {
    color: #666;
}

.resh {
    color: #fe8433;
}

.cell {
    width: 100%;
    padding: 30rpx;
    background-color: #fff;
}

.topPic {
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
}

.title {
    font-size: 32rpx;
    color: #333;
    line-height: 36rpx;
}

.labBox {
    line-height: 18px;
}

.label {
    text-align: center;
    color: #fff;
    font-size: 24rpx;
    padding: 0 14rpx;
    max-width: 50%;
    border-radius: 6rpx;
}

.telNum {
    padding-top: 30rpx;
}

.descBox {
    margin-top: 50rpx;
    margin-bottom: 112rpx;
    padding: 30rpx;
}

.descCon {
    text-indent: 2em;
    font-size: 28rpx;
    line-height: 40rpx;
}

.addBox .f16 {
    line-height: 40rpx;
}

.descContent {
    line-height: 40rpx;
}

.descPic {
    display: inline-block;
    width: 31.33333333333%;
    height: 200rpx;
    margin-left: 1%;
    margin-right: 1%;
    margin-bottom: 10rpx;
}

.icon-weixin {
    color: #09bb07;
}