}

.topBox {
    padding: 20rpx 30rpx;
}

.seaBox {
    line-height: 68rpx;
    border-radius: 10rpx;
}

.tabBox {
    color: #666;
}

.tabBox .sub {
    position: relative;
    display: inline-block;
    width: 30%;
    margin: 0 1.666666666%;
    padding: 30rpx 0;
}

.tabBox .sub .picker {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.tabBox .iconfont {
    font-size: 24rpx;
    color: #999;
}