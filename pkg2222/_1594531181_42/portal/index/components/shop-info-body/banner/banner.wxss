.wrap {
    width: 100%;
    box-sizing: border-box;
    border: 1px solid #E5E5E5;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.banner-img {
    width: 432rpx;
    height: 253rpx;
}

.banner-right-part {
    display: flex;
    flex-direction: column;
    padding-left: 48rpx;
    max-width: 184.78rpx;
}

.banner-right-part-text0 {
    font-size: 12px;
    color: #888;
    line-height: 34rpx;
}

.banner-right-part-text1 {
    font-size: 15px;
    color: #353535;
}

.banner-right-part-text-separator {
    margin-top: 32rpx;
    margin-bottom: 16rpx;
    width: 80rpx;
    height: 2rpx;
    background: #D8D8D8;
}