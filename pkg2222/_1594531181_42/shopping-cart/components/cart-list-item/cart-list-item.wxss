@font-face {
    font-family: 'WeChat Sans Std';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.container {
    width: 100%;
    overflow: hidden;
    background: #fff;
}

.container .wrapper {
    position: relative;
    box-sizing: border-box;
    width: 100%;
    z-index: 2;
    display: flex;
    flex-direction: column;
    justify-content: left;
    align-items: left;
    background: #fff;
}

.container .wrapper.hover {
    background: #f2f2f2;
}

.container .wrapper .wrapper-item-info {
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    padding-top: 44rpx;
    padding-bottom: 41rpx;
    width: 100%;
}

.container .wrapper .wrapper-item-info.with-line {
    padding-bottom: 61rpx;
}

.container .wrapper .wrapper-item-info.hover {
    background: #f0f0f0;
}

.container .wrapper .wrapper-item-info::after {
    position: absolute;
    content: "";
    left: 30rpx;
    bottom: 0;
    width: 100%;
    height: auto;
    border-bottom: 0.5px solid #e5e5e5;
}

.container .wrapper .wrapper-item-info .wrapper-image-body {
    position: relative;
    width: 100%;
    height: auto;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.container .wrapper .wrapper-item-info .item-image-container {
    box-sizing: border-box;
    display: inline-block;
    width: 500rpx;
    height: 320rpx;
}

.container .wrapper .wrapper-item-info .item-image-container .item-image {
    width: 500rpx;
    height: 320rpx;
}

.container .wrapper .wrapper-item-info .item-image-container.item-image-container-hover {
    opacity: 0.5;
}

.container .wrapper .wrapper-item-info .line {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    border-bottom: 20rpx solid #f5f5f5;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail {
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: top;
    font-size: 32rpx;
    width: 100%;
    padding-bottom: 24rpx;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info {
    margin-top: 40rpx;
    text-align: center;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .title {
    font-weight: bold;
    font-size: 30rpx;
    color: #000;
    line-height: 36rpx;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .detail {
    margin-top: 30rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .detail .detail__desc {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .detail .detail__left_count {
    margin-top: 16rpx;
    color: #888;
    font-size: 24rpx;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .detail .price {
    box-sizing: border-box;
    color: #888;
    font-size: 28rpx;
    font-family: "WeChat Sans Std";
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .detail .price-sep-detail {
    margin-left: 20rpx;
    margin-right: 20rpx;
    border-left: 1px solid #D8D8D8;
    width: 0;
    height: 22rpx;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .detail .desc {
    box-sizing: border-box;
    color: #888;
    font-size: 26rpx;
    max-width: 70%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-info .detail .no-padding {
    padding-left: 0;
    padding-right: 0;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-link {
    display: flex;
    justify-content: center;
    align-items: center;
    padding-top: 48rpx;
    padding-bottom: 48rpx;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-link.hover {
    opacity: 0.5;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-link .btn-buy {
    color: #000;
    display: inline-block;
    font-size: 26rpx;
    background-size: auto 26rpx;
    opacity: 0.8;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-link .btn-buy.light {
    color: #888;
}

.container .wrapper .wrapper-item-info .wrapper-item-info-detail .wrapper-link image {
    margin-left: 12.2rpx;
    width: 11rpx;
    height: 19.2rpx;
}

.container .wrapper .wrapper-select-area {
    position: absolute;
    left: 0;
    width: 125rpx;
    height: auto;
    padding: 20rpx 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    top: 50%;
    transform: translate(0,-50%);
}

.container .wrapper .wrapper-select-area image {
    background-size: 40rpx auto;
    width: 40rpx;
    height: 40rpx;
}

.detail__action {
    margin-top: 20rpx;
    width: 80px;
    height: 24px;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: rgba(80,159,201,0.10);
    border-radius: 2px;
}

.detail__action-text {
    font-size: 12px;
    color: #509FC9;
    letter-spacing: 0.19px;
    text-align: center;
    line-height: 24px;
    height: 24px;
}