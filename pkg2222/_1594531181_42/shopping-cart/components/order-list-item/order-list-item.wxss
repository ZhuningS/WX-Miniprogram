@font-face {
    font-family: 'WeChat Sans Std';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.container {
    position: relative;
    width: 100%;
    overflow: hidden;
    border-bottom: 16rpx solid #f7f7f7;
}

.container .order {
    position: relative;
    box-sizing: border-box;
    width: 100%;
    z-index: 2;
    flex-direction: column;
    background: #fff;
}

.container .order .header {
    display: flex;
    flex-direction: row;
    height: 108rpx;
    justify-content: center;
    align-items: center;
    border-bottom: 0.5rpx solid #ececec;
    padding-left: 64rpx;
}

.container .order .header .mch-logo-image {
    width: 48rpx;
    height: 48rpx;
    border-radius: 48rpx;
    margin-right: 20rpx;
    background: #fbfbfb;
}

.container .order .header .mch-name {
    flex: 1;
    font-size: 28rpx;
    color: #000000;
    line-height: 108rpx;
    box-sizing: border-box;
}

.container .order .list {
    height: 100%;
    padding-left: 64rpx;
}

.container .order .list .item {
    display: flex;
    flex-direction: row;
    padding-top: 50rpx;
    padding-bottom: 48rpx;
}

.container .order .list .item .item-img {
    width: 162rpx;
    height: 162rpx;
}

.container .order .list .item .info {
    flex: 1;
    box-sizing: border-box;
    padding-left: 40rpx;
    padding-right: 60rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.container .order .list .item .info .title {
    font-size: 28rpx;
    color: #000000;
    padding-bottom: 24rpx;
}

.container .order .list .item .info .wrap-price-quantity {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.container .order .list .item .info .wrap-price-quantity .quantity {
    color: #888;
    font-size: 26rpx;
    text-align: right;
    flex: 1;
}

.container .order .list .item .info .wrap-price-quantity .price {
    font-family: "WeChat Sans Std";
    font-weight: bold;
    font-size: 30rpx;
    color: #000;
}

.container .order .list .item .info .desc {
    font-size: 26rpx;
    color: #B2B2B2;
    box-sizing: border-box;
    padding-bottom: 30rpx;
}

.container .order .list .item .info .wrap-price {
    display: flex;
    flex-direction: row;
}

.container .order .list .item .info .wrap-price .unit-price {
    flex: 1;
    color: #000;
    font-size: 38rpx;
}

.container .order .list .item .info .wrap-price .quantity {
    display: inline-block;
    width: 128rpx;
    text-align: right;
    color: #a0a0a0;
    font-size: 38rpx;
}

.container .order .list .dash {
    height: 1px;
    background: linear-gradient(to right,#f2f2f2,#f2f2f2 6px,transparent 6px,transparent);
    background-size: 12px 1px;
}

.container .order .summary {
    display: flex;
    flex-direction: column;
    margin-bottom: 32rpx;
    padding-right: 32rpx;
}

.container .order .summary .main {
    display: inline-block;
    width: 100%;
    text-align: right;
    font-size: 30rpx;
}

.container .order .summary .sub {
    display: inline-block;
    width: 100%;
    text-align: right;
    font-size: 30rpx;
    color: #a0a0a0;
}

.container .hover {
    background: #f2f2f2;
}