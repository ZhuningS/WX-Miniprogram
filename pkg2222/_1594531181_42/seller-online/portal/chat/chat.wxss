.page-bg {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    position: fixed;
    background: #F1F1F1;
}

.custom-navigation-bar {
    position: fixed;
    z-index: 2000;
    width: 100%;
    height: 44px;
    left: 0;
    top: 20px;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.custom-navigation-bar.iPhoneX {
    top: 44px!important;
}

.custom-navigation-bar.android {
    top: 28px!important;
}

.home-btn {
    width: 46rpx;
    height: 42rpx;
    margin-left: 32rpx;
}

.wrap-list {
    position: relative;
}

.list {
    background: #F1F1F1;
    box-sizing: border-box;
}

.list.back {
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    z-index: -1;
}

.list-bottom {
    height: 10px;
}

.bottom {
    background: #F5F5F7;
    position: fixed;
    width: 750rpx;
    height: 532rpx;
    border-top: 1px solid #E5E5E5;
}

.bottom-input {
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 100rpx;
    padding: 0 24rpx;
}

.bottom-input.iPhoneX {
    padding-bottom: 50rpx;
}

.input {
    background: #FFF;
    flex-grow: 1;
    height: 74rpx;
    margin: 13rpx 10rpx 13rpx 0;
    padding-left: 15rpx;
    border-radius: 6rpx;
}

.input-tools-btn {
    width: 70rpx;
    height: 70rpx;
    padding: 2rpx;
    background-size: 70rpx 70rpx;
    background-image: url('data:image/png;base64,');
}

.input-tools-btn_hover {
    background-image: url('data:image/png;base64,');
}

.accept-service-btn {
    display: flex;
    width: 750rpx;
    height: 74rpx;
    margin: 13rpx 10rpx;
}

.accept-service-btn__text {
    margin: auto;
    font-size: 30rpx;
    color: #333;
}

.bottom-tools {
    position: absolute;
    left: 0;
    top: 100rpx;
    width: 100%;
    height: auto;
    box-sizing: border-box;
    padding: 30rpx 56rpx 52rpx 56rpx;
    display: flex;
    flex-direction: column;
    border-top: 1px solid #E5E5E5;
}

.bottom-tools.iPhoneX {
    padding-bottom: 72rpx;
}

.bottom-tools-line {
    width: 100%;
    padding-top: 22rpx;
    display: flex;
    flex-direction: row;
}

tools-attachment {
    flex-grow: 1;
}

.send-image-shortcut {
    position: fixed;
    right: 10rpx;
    bottom: 110rpx;
    width: 168rpx;
    height: 220rpx;
    background: transparent;
    overflow: hidden;
}

.send-image-shortcut.iPhoneX {
    bottom: 160rpx;
}

.shortcut_inner {
    background: #FFF;
    height: calc(100% - 5px);
    border-radius: 4rpx;
    display: flex;
    flex-direction: column;
    border: 1rpx solid #E5E5E5;
}

.shortcut_inner__text {
    font-size: 24rpx;
    color: #000;
    padding: 10rpx 10rpx 5rpx;
}

.shortcut_inner__image {
    width: 148rpx;
    height: 118rpx;
    margin: 0 auto;
    background: #EBEBEB;
    overflow: hidden;
}

.shortcut_arrow {
    width: 0;
    height: 0;
    border: 10rpx solid;
    border-color: transparent;
    border-top-color: #FFF;
    margin-left: auto;
    margin-right: 40rpx;
    transform: translateY(-1rpx);
}

.load_more {
    display: flex;
}

.load_more_inner {
    margin: 20rpx auto;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.load_more__icon {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.load_more__tip {
    font-size: 28rpx;
    color: #888;
}