.wrap {
    margin: 0 auto;
    width: 128rpx;
    height: auto;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.wrap-icon {
    border-radius: 20rpx;
    background: #FFF;
    width: 128rpx;
    height: 128rpx;
}

.icon {
    width: 64rpx;
    height: 64rpx;
    padding: 32rpx;
}

.title {
    margin-top: 10rpx;
    font-size: 26rpx;
    color: #888;
}