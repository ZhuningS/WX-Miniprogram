.wrap {
    top: 0;
    left: 0;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.warning-text {
    margin-top: 90rpx;
    color: #8c8c8c;
    font-size: 16px;
}

.error-wrap {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.error-icon-area {
    background-image: url(data:image/png;base64,);
    background-size: 100px auto;
    background-repeat: no-repeat;
    width: 100px;
    height: 100px;
}

.error-title {
    margin-top: 24rpx;
    margin: 24rpx;
    color: #000;
    font-size: 16px;
}

.error-subtitle {
    color: #8c8c8c;
    font-size: 16px;
}