.address-item {
    position: relative;
    padding-left: 60rpx;
    padding-right: 46rpx;
}

.address-item::after {
    position: absolute;
    left: 60rpx;
    bottom: 0rpx;
    content: "";
    width: 690rpx;
    height: 1rpx;
    transform: scaleY(0.5);
    background: #E5E5E5;
}

.no_border {
}

.no_border::after {
    opacity: 0;
}

.address-item-inner {
    display: flex;
    flex-direction: row;
    padding-top: 60rpx;
    padding-bottom: 52rpx;
}

.address-item-content {
    width: 458rpx;
    display: flex;
    flex-direction: column;
}

.address-item-content-title {
    opacity: 0.9;
    font-size: 16px;
    font-family: PingFangSC-Medium;
    color: #000;
    margin-bottom: 22rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.address-item-content-subtitle {
    font-size: 13px;
    color: #888;
    line-height: 40rpx;
    max-height: 80rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-top: 14rpx;
}

.address-item-right-part {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    width: 159rpx;
    height: 40rpx;
    margin-left: 34rpx;
    margin-top: 4rpx;
}

.address-item-right-part-icon {
    width: 40rpx;
    height: 40rpx;
    background-size: 40rpx 40rpx;
    background-repeat: no-repeat;
}

.address-item-right-part-separator {
    width: 1rpx;
    height: 40rpx;
    background: #000;
    opacity: 0.08;
}

.address-info-error {
    position: relative;
    display: block;
}

.address-item-right-part-icon__call {
    background-image: url("data:image/png;base64,");
}

.address-item-right-part-icon__call:active {
    background-image: url("data:image/png;base64,");
}

.address-item-right-part-icon__locate {
    background-image: url("data:image/png;base64,");
}

.address-item-right-part-icon__locate:active {
    background-image: url("data:image/png;base64,");
}