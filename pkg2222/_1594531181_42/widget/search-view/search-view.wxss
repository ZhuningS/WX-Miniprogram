@import "../../__wuBaseWxss__/3.wxss";

@import "../../__wuBaseWxss__/2.wxss";

.search-wrap {
    border-radius: 8rpx;
    background: #F7F7F7;
    position: fixed;
    z-index: 10000;
    left: 80rpx;
    top: 26px;
    width: 360rpx;
    padding: 6rpx 16px;
    display: flex;
}

.search-wrap.iPhoneX {
    top: 49px!important;
}

.search-wrap.android {
    top: 34px!important;
}

.search-input {
    font-size: 14px;
    color: #353535;
    margin-left: 14px;
}

.search-mask {
    opacity: 0;
    background: #000000;
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 500;
    left: 0;
    right: 0;
}

.search-teach {
    position: fixed;
    z-index: 10000;
    background: #FFFFFF;
    opacity: 0;
    width: 100%;
}

.search-teach__history-item {
    padding: 0;
    font-size: 14px;
    color: #555;
}

.search-teach__icon {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: 0 50%;
    background-size: 18px 18px;
    padding: 25px 13px;
    margin-left: 46px;
}

.history-item__content {
    overflow: hidden;
    padding-left: 0;
    font-size: 15px;
    color: #000;
    padding: 15px;
}

.history_item__text {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 100%;
}

.delete {
    width: 13px;
    height: 13px;
    margin-left: 0;
    padding: 15px 25px;
}

.clear {
    width: 14px;
    height: 14px;
}