.__indexExchange {
    padding: 20rpx;
}

.list {
    overflow: hidden;
    background: #fff;
    border-radius: 15rpx;
    padding: 20rpx;
    margin-bottom: 15rpx;
}

.list image {
    width: 150rpx;
    height: 150rpx;
    float: left;
    margin-right: 20rpx;
}

.listText {
}

.title {
    color: #333;
    font-size: 32rpx;
    font-weight: 600;
}

.tip {
    vertical-align: bottom;
    overflow: hidden;
    margin-top: 60rpx;
}

.tip text {
    width: 33%;
    display: inline-block;
}

.status {
    font-size: 28rpx;
}

.text-green {
    color: #27ccaa;
}

.text-red {
    color: #fb6262;
}

.rest {
    color: #666;
    font-size: 24rpx;
}

.listBtn {
    border: 1px solid #27ccaa;
    background-color: rgb(39, 204, 170);
    width: 120rpx !important;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 25rpx;
    font-size: 24rpx;
    color: #fff;
}

.modal-box {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0px;
    background: rgba(0, 0, 0, 0.4);
    overflow: hidden;
}

.modal-body {
    position: relative;
}

.modal-content {
    width: 80%;
    height: 600rpx;
    margin: 20% auto;
    overflow: hidden;
}

.modal-content .fenxiang {
    width: 100%;
    height: 100%;
}

.canvas-box{position: fixed;top:999999rpx;left: 0}