.c-label {
    height: 32rpx;
    overflow: hidden;
}

.c-label>text {
    min-width: 50rpx;
    max-width: 116rpx;
    text-align: center;
    font-size: 20rpx;
    color: #666;
    background-color: #eee;
    padding: 0 5rpx;
    height: 32rpx;
    line-height: 32rpx;
    border-radius: 5rpx;
    margin-right: 10rpx;
}