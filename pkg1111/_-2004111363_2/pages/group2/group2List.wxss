.c-gpOrderLst {
    width: 100%;
}

.c-gpOrderLst .c-gpOrderLst-top {
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
}

.c-gpOrderLst .c-gpOrderLst-top>view {
    font-size: 30rpx;
    width: 33.3%;
    text-align: center;
}

.c-gpOrderLst .c-gpOrderLst-main {
    width: 100%;
    margin-top: 20rpx;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item:last-child {
    margin-bottom: 20rpx;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item {
    width: 100%;
    padding-left: 30rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item .c-gpOrder-item-top {
    width: 100%;
    border-bottom: 1px solid #eee;
    padding: 30rpx 30rpx 30rpx 0;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item .c-gpOrder-pro {
    width: 100%;
    padding: 30rpx 30rpx 30rpx 0;
    border-bottom: 1px solid #eee;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item .c-gpOrder-pro>image {
    width: 130rpx;
    height: 130rpx;
    flex-shrink: 0;
    margin-right: 20rpx;
    border-radius: 5rpx;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item .c-gpOrder-item-bottom {
    width: 100%;
    padding: 30rpx 30rpx 30rpx 0;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item .c-gpOrder-item-bottom .c-gpOrder-item-btn {
    width: 100%;
    margin-top: 50rpx;
}

.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item .c-gpOrder-item-bottom .c-gpOrder-item-btn>text,.c-gpOrderLst .c-gpOrderLst-main .c-gpOrder-item .c-gpOrder-item-bottom .c-gpOrder-item-btn .paybtn {
    width: 146rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    border-radius: 25rpx;
    font-size: 26rpx;
    border: 1px solid #a1a2a3;
}