        <view bindtap="$comLogo$makeMinapp" class="do-minapp skin_br" data-com-index="{{index}}" data-wpymakeminapp-a="{{vm_com_logo.QrcodeId}}" wx:if="{{vm_com_logo.QrcodeId>0&&vm_com_logo.OpenExtension==0}}">我也要做小程序</view>
    </view>
    <view class="masking w-100 h-100 f fv fc fc-h" wx:if="{{coupHidden}}">
        <view class="coupfloat-box rel ">
            <image class="header abs" src="http://j.vzan.cc/miniapp/img/enterprise/coup_header.png"></image>
            <view class="coup-item w-100 h-100 f fc" wx:if="{{$comCouponsFloat$coupIndex<3}}" wx:for="{{vm_com_coupList}}" wx:for-index="$comCouponsFloat$coupIndex" wx:for-item="$comCouponsFloat$item" wx:key="$comCouponsFloat$key">
                <view class="info h-100 f fv fc-h pl20">
                    <view class="title">{{$comCouponsFloat$item.CouponName}}</view>
                    <view class="money f26" wx:if="{{$comCouponsFloat$item.CouponWay==0}}">{{$comCouponsFloat$item.Money_fmt}}元</view>
                    <view class="money f26" wx:else>{{$comCouponsFloat$item.Money_fmt}}折</view>
                </view>
                <view class="btn-box h-100 f fc fv fc-h">
                    <view catchtap="$comCouponsFloat$getCoup" class="btn tc cfff f24" data-com-index="{{index}}" data-wpygetcoup-a="{{$comCouponsFloat$item.Id}}" data-wpygetcoup-b="{{$comCouponsFloat$coupIndex}}" wx:if="{{$comCouponsFloat$item.CanGet}}">{{$comCouponsFloat$item.coupBtnText}}</view>
                    <view class="get-text f22" wx:if="{{$comCouponsFloat$item.PersonNum>0}}">{{$comCouponsFloat$item.PersonNum}}人已领</view>
                </view>
            </view>
            <view catchtap="$comCouponsFloat$closeMask" class="dzicon icon-zhuanyebaniconkuozhan_Close cfff f60 abs coupfloat-close" data-com-index="{{index}}"></view>
        </view>
    </view>
    <view class="masking w-100 h-100 f fv fc fc-h" wx:if="{{signinRules.State==1&&showsigninFloat}}">
        <view class="signin-wrap bg-w rel f fv fc">
            <view bindtap="$comSigninFloat$closeSigninMask" class="close abs dzicon icon-zhuanyebaniconkuozhan_Close cfff f60" data-com-index="{{index}}"></view>
            <image mode="aspectFill" src="http://j.vzan.cc/miniapp/img/enterprise/sign-in-bg.png"></image>
            <view class="signin-day abs f28">已连续签到<text style="color:#FFD530">{{userSignin.ConnectDay}}</text>天</view>
            <view class="date-list rel f fc fj">
                <view class="date-item f fv fc" wx:for="{{userSignin.listPlayCardLog}}" wx:for-index="$comSigninFloat$index" wx:for-item="$comSigninFloat$item" wx:key="$comSigninFloat${{$comSigninFloat$index}}">
                    <view class="rel">
                        <image mode="aspectFill" src="{{$comSigninFloat$item.Played?'http://j.vzan.cc/miniapp/img/enterprise/sign-in.png':'http://j.vzan.cc/miniapp/img/enterprise/signed-in.png'}}" style="width:50rpx;height:50rpx;"></image>
                        <text class="abs integral" wx:if="{{!$comSigninFloat$item.Played}}">+{{$comSigninFloat$item.Points}}</text>
                    </view>
                    <text class="date c666 f20">{{$comSigninFloat$item.dateStr}}</text>
                </view>
            </view>
            <view class="f fc fc-h">
                <view bindtap="$comSigninFloat$clickSignin" class="signin-btn f30" data-com-index="{{index}}" wx:if="{{!userSignin.TodayPlayCard}}">签到</view>
                <view class="signed-btn f30" wx:if="{{userSignin.TodayPlayCard}}">今天已签到</view>
            </view>
        </view>
    </view>
    <view class="c-vas-mask" hidden="{{!showCanvas?'hidden':''}}">
        <view bindtap="$comVas$hidemask" class="c-vas-bg"></view>
        <view bindtap="$comVas$hidemask" class="dzicon icon-zhuanyebaniconkuozhan_Close c-vas-cancle"></view>
        <canvas canvasId="firstCanvas" class="c-vas-main"></canvas>
        <view class="c-vas-bottom f fc fsa">
            <view bindtap="$comVas$saveQrcode" class="c-vas-btn f fv fc" data-wpysaveqrcode-a="0" data-wpysaveqrcode-b="{{$comVas$vm}}">
                <view class="dzicon icon-Choice_xuanze  c-vas-save"></view>
                <text>保存图片</text>
            </view>
            <view class="c-vas-btn f fv fc">
                <button class="dzicon icon-Forward_fenxiang c-vas-share" openType="share"></button>
                <text>分享给朋友</text>
            </view>
        </view>
    </view>
    <wxc-toast isShow="{{toast.show}}" text="{{toast.msg}}"></wxc-toast>
    <view style="height:100rpx"></view>
</view>
