<view class="{{currentSkin}} {{showCanvas?'page-none':''}}">
    <view style="height:100rpx;" wx:if="{{hasMusic}}"></view>
    <block wx:for="{{currentPage.coms}}" wx:key="index">
        <view class="c-music" wx:if="{{item.type=='bgaudio'}}">
            <view class="c-music-main f fc fj" style="opacity:{{item.scrollShow?'0.75':'1'}} ">
                <view class="f fc">
                    <image class="c-music-img {{item.playAudio?'rotate':''}}" src="{{item.img}}"></image>
                    <text class="f24 ">{{item.title}}</text>
                </view>
                <view bindtap="$comMusic$stopMusic" class="dzicon icon-zanting-copy f44" data-com-index="{{index}}" data-wpystopmusic-a="{{currentPageIndex}}" wx:if="{{item.playAudio}}"></view>
                <view bindtap="$comMusic$playMusic" class="dzicon icon-bofang1-copy f44" data-com-index="{{index}}" data-wpyplaymusic-a="{{item.src}}" data-wpyplaymusic-b="{{currentPageIndex}}" wx:if="{{!item.playAudio}}"></view>
            </view>
        </view>
        <ad unitId="{{item.unitid}}" wx:if="{{item.type=='ad'}}"></ad>
        <official-account wx:if="{{item.type=='attention'}}"></official-account>
        <view class="c-map" wx:if="{{item.type=='map'}}">
            <view bindtap="$comMap$openMap" class="c-map__main f fj fc" data-com-index="{{index}}" data-wpyopenmap-a="{{item}}">
                <view class="p f fj fc">
                    <text class="dzicon icon-dingwei f40 p-img"></text>
                    <text class="line line1 ml20">{{item.txt}}</text>
                </view>
                <view class="dzicon icon-xiala-copy-right f38 c123"></view>
            </view>
        </view>
        <view bindtap="$comVideo$goPlay" class="com-video" data-com-index="{{index}}" data-wpygoplay-a="{{item.src}}" wx:if="{{item.type=='video'}}">
            <image class="v-img" src="{{item.poster}}" wx:if="{{item.poster}}"></image>
            <view class="v-bg" wx:else></view>
            <image class="v-play" mode="aspectFill" src="http://j.vzan.cc/miniapp/img/enterprise/videoplay.png"></image>
        </view>
        <view class="w-100" style="height: {{item.spacing+'px'}}" wx:if="{{item.type=='spacing'}}"></view>
        <navigator hoverClass="none" openType="navigate" url="/pages/good/goodSearch" wx:if="{{item.type=='search'}}">
            <view class="c-search skin_bg">
                <view class="c-search-main f fc">
                    <text class="c-search-main-icon dzicon icon-sousuo_rearch"></text>
                    <text>{{item.placeholder}}</text>
                </view>
            </view>
        </navigator>
        <view class="com-imgnav f fc bg-w fw fj" wx:if="{{item.type=='imgnav'}}">
            <view bindtap="$comImgnav$navPage" class="imgNav f fc fj fv" data-com-index="{{index}}" data-wpynavpage-a="{{$comImgnav$item}}" wx:if="{{$comImgnav$item.icon!=''||$comImgnav$item.img!=''||$comImgnav$item.name!=''}}" wx:for="{{item.navlist}}" wx:for-index="$comImgnav$index" wx:for-item="$comImgnav$item" wx:key="$comImgnav$key">
                <image mode="scaleToFill" src="{{$comImgnav$item.img}}" wx:if="{{$comImgnav$item.img!=''}}"></image>
                <text class="dzicon {{$comImgnav$item.icon}} f80" wx:if="{{$comImgnav$item.img==''}}"></text>
                <text class="f26">{{$comImgnav$item.name}}</text>
            </view>
        </view>
        <view class="mac" wx:if="{{item.type=='magicCube'}}">
            <view class="f" wx:if="{{item.style==0}}">
                <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[0]}}" mode="widthFix" src="{{item.items[0].img}}"></image>
                <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[1]}}" mode="widthFix" src="{{item.items[1].img}}" style="margin-left:{{item.imgSpacing}}px"></image>
            </view>
            <view class="f" wx:elif="{{item.style==1}}">
                <image bindtap="$comMagic$imgFunc" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[0]}}" mode="widthFix" src="{{item.items[0].img}}"></image>
                <image bindtap="$comMagic$imgFunc" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[1]}}" mode="widthFix" src="{{item.items[1].img}}" style="margin-left:{{item.imgSpacing}}px"></image>
                <image bindtap="$comMagic$imgFunc" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[2]}}" mode="widthFix" src="{{item.items[2].img}}" style="margin-left:{{item.imgSpacing}}px"></image>
            </view>
            <view class="f" wx:elif="{{item.style==2}}">
                <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[0]}}" mode="widthFix" src="{{item.items[0].img}}"></image>
                <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[1]}}" mode="widthFix" src="{{item.items[1].img}}" style="margin-left:{{item.imgSpacing}}px"></image>
                <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[2]}}" mode="widthFix" src="{{item.items[2].img}}" style="margin-left:{{item.imgSpacing}}px"></image>
                <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[3]}}" mode="widthFix" src="{{item.items[3].img}}" style="margin-left:{{item.imgSpacing}}px"></image>
            </view>
            <block wx:elif="{{item.style==3}}">
                <view class="f">
                    <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[0]}}" mode="widthFix" src="{{item.items[0].img}}"></image>
                    <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[1]}}" mode="widthFix" src="{{item.items[1].img}}" style="margin-left:{{item.imgSpacing}}px"></image>
                </view>
                <view class="f">
                    <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[2]}}" mode="widthFix" src="{{item.items[2].img}}" style="margin-top:{{item.imgSpacing}}px"></image>
                    <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[3]}}" mode="widthFix" src="{{item.items[3].img}}" style="margin-left:{{item.imgSpacing}}px;margin-top:{{item.imgSpacing}}px"></image>
                </view>
            </block>
            <view class="f" wx:elif="{{item.style==4}}">
                <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[0]}}" mode="widthFix" src="{{item.items[0].img}}" style="max-width:375rpx;"></image>
                <view class="f f1 rel" style="margin-left:{{item.imgSpacing}}px">
                    <view class="abs f fv" style="top:0;bottom:0;overflow:hidden;">
                        <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[1]}}" mode="aspectFill" src="{{item.items[1].img}}" style="max-width:375rpx;"></image>
                        <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[2]}}" mode="aspectFill" src="{{item.items[2].img}}" style="margin-top:{{item.imgSpacing}}px;max-width:375rpx;"></image>
                    </view>
                </view>
            </view>
            <block wx:elif="{{item.style==5}}">
                <view class="f">
                    <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[0]}}" mode="widthFix" src="{{item.items[0].img}}"></image>
                </view>
                <view class="f" style="margin-top:{{item.imgSpacing}}px">
                    <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[1]}}" mode="widthFix" src="{{item.items[1].img}}"></image>
                    <image bindtap="$comMagic$imgFunc" class="f1" data-com-index="{{index}}" data-wpyimgfunc-a="{{item.items[2]}}" mode="widthFix" src="{{item.items[2].img}}" style="margin-left:{{item.imgSpacing}}px;"></image>
                </view>
            </block>
        </view>
        <image bindtap="$comImg$imgPlay" class="w-100" data-com-index="{{index}}" data-wpyimgplay-a="{{item}}" mode="widthFix" src="{{item.imgurl_fmt}}" wx:if="{{item.type=='img'&&item.imgurl}}"></image>
        <swiper indicatorDots autoplay="true" class="com-slider w-100" current="{{item.current}}" duration="300" interval="5000" wx:if="{{item.type=='slider'&&item.items[0].img}}">
            <swiper-item wx:for="{{item.items}}" wx:for-index="$comSlider$index" wx:for-item="$comSlider$swiper" wx:key="$comSlider$key">
                <image bindtap="$comSlider$imgFunc" class="w-100 h400" data-com-index="{{index}}" data-wpyimgfunc-a="{{$comSlider$swiper}}" src="{{$comSlider$swiper.img_fmt}}"></image>
            </swiper-item>
        </swiper>
        <view class="c-coup" wx:if="{{item.type=='coupons'&&item.items.length}}">
            <view class="component-title f fc fc-h">
                <text class="component-title-left dzicon icon-_xiegan skin_txt"></text>优惠券<text class="component-title-right dzicon icon-_xiegan skin_txt"></text>
            </view>
            <scroll-view>
                <view class="c-coup-main f">
                    <view class="c-coup-main-item f  fj" wx:if="{{$comCoupons$index<3}}" wx:for="{{item.items}}" wx:for-index="$comCoupons$index" wx:for-item="$comCoupons$item" wx:key="$comCoupons$key">
                        <view class="f fv fj mr10">
                            <text class="c-coup-main-item-title">{{$comCoupons$item.CouponName}}享受{{$comCoupons$item.Money}}{{$comCoupons$item.CouponWay==0?'元':'折'}}优惠</text>
                            <view>
                                <text class="c-coup-main-item-Lit">{{$comCoupons$item.LimitMoney=='0'?'无门槛使用':'满'+$comCoupons$item.LimitMoney+'元使用'}}</text>
                            </view>
                            <view wx:if="{{$comCoupons$item.ValType==0}}">
                                <view class="c-coup-main-item-tip">{{$comCoupons$item.StartUseTime}} -</view>
                                <view class="c-coup-main-item-tip">{{$comCoupons$item.EndUseTime}}</view>
                            </view>
                            <text class="c-coup-main-item-tip" wx:if="{{$comCoupons$item.ValType==1||$comCoupons$item.ValType==2}}">领券{{$comCoupons$item.ValType==1?'次日':'当日'}}开始{{$comCoupons$item.ValDay}}天内有效</text>
                        </view>
                        <view class="f fv fc fj">
                            <view class="c-coup-main-item-tip f fv">
                                <text>满足条件</text>
                                <text>即可使用</text>
                            </view>
                            <view bindtap="$comCoupons$getCoupon" class="c-coup-main-item-btn f fc fc-h" data-com-index="{{index}}" data-wpygetcoupon-a="{{$comCoupons$item.Id}}">立即领取</view>
                        </view>
                    </view>
                </view>
            </scroll-view>
            <view bindtap="$comCoupons$goMyCoupons" class="c-coup-btn" data-com-index="{{index}}" wx:if="{{item.items.length>3}}">查看全部</view>
        </view>
        <view class="c-contact f fc fj" wx:if="{{item.type=='contactShopkeeper'&&item.pageShow}}">
            <view class="f fc">
                <image class="c-contact-logo" src="{{item.iconType==1&&item.iconUrl?item.iconUrl:item.shopLogo_url}}"></image>
                <view class="line line1">{{item.txt||'欢迎光临本店，我是店主'}}</view>
            </view>
            <view class="f fc">
                <view bindtap="$comContact$phone" class="f fv fc" data-com-index="{{index}}" data-wpyphone-a="{{item.phoneNum}}" wx:if="{{item.openTel}}">
                    <text class="dzicon icon-phone-channel f40" style="color:#39ADFF;"></text>
                    <text class="f22 mt10 c666">电话</text>
                </view>
                <button class="f fv fc" hoverClass="none" openType="contact" style="margin-left:50rpx" wx:if="{{item.openService&&item.serverType=='wxServer'}}">
                    <text class="dzicon icon-weixin f40" style="color:#86CE39"></text>
                    <text class="f22 mt10 c666">微信</text>
                </button>
                <view bindtap="$comContact$gochat" class="f fv fc" data-com-index="{{index}}" style="margin-left:50rpx" wx:if="{{item.openService&&item.serverType=='miniappServer'}}">
                    <text class="dzicon icon-weixin f40" style="color:#86CE39"></text>
                    <text class="f22 mt10 c666">微信</text>
                </view>
            </view>
        </view>
        <form bindsubmit="$comForm$sumbitFormFuc" data-com-index="{{index}}" data-wpysumbitformfuc-a="{{item.title}}" reportSubmit="true" wx:if="{{item.type=='form'}}">
            <view class="c-form">
                <view class="component-title f fc fc-h" wx:if="{{item.title}}">
                    <text class="component-title-left dzicon icon-_xiegan skin_txt"></text>{{item.title}}<text class="component-title-right dzicon icon-_xiegan skin_txt"></text>
                </view>
                <view class="c-form-main">
                    <block wx:for="{{item.items}}" wx:for-index="$comForm$index" wx:for-item="$comForm$item" wx:key="$comForm$key">
                        <view class="c-form-main-name f fv" wx:if="{{$comForm$item.type=='text'}}">
                            <text class="f26">{{$comForm$item.name}}</text>
                            <input bindinput="$comForm$inputFy" data-com-index="{{index}}" data-wpyinputfy-a="{{$comForm$index}}" name="{{$comForm$item.name}}" placeholder="{{$comForm$item.placeholder}}" placeholderClass="c99 f24" type="{{$comForm$item.type}}" value="{{$comForm$vm.name[$comForm$index]}}"></input>
                        </view>
                        <view class="c-form-main-name f fv" wx:if="{{$comForm$item.type=='number'}}">
                            <text class="f26">{{$comForm$item.name}}</text>
                            <input bindinput="$comForm$inputFp" data-com-index="{{index}}" data-wpyinputfp-a="{{$comForm$index}}" name="{{$comForm$item.name}}" placeholder="{{$comForm$item.placeholder}}" placeholderClass="c99 f24" type="{{$comForm$item.type}}" value="{{$comForm$vm.phone[$comForm$index]}}"></input>
                        </view>
                        <view class="c-form-main-name f fv" wx:if="{{$comForm$item.type=='radio'}}">
                            <text class="f26">{{$comForm$item.name}}</text>
                            <picker bindcancel="$comForm$pickCancle" bindchange="$comForm$pickFunc" bindtap="$comForm$pickShow" class="w-100 rel" data-com-index="{{index}}" data-wpypickcancle-a="{{$comForm$index}}" data-wpypickfunc-a="{{$comForm$index}}" data-wpypickshow-a="{{$comForm$index}}" range="{{$comForm$item.array}}" value="{{$comForm$vm.pickIndex[$comForm$index]}}">
                                <input disabled class="pickItem" name="{{$comForm$item.name}}" placeholder="{{$comForm$item.name}}" placeholderClass="c99 f24" type="text" value="{{$comForm$item.array[ $comForm$vm.pickIndex[$comForm$index] ]}}"></input>
                                <text class="dzicon icon-xialax c-form-icon {{$comForm$vm.showXia[$comForm$index]?'c-form-iconChange':'c-form-iconHidden'}}"></text>
                            </picker>
                        </view>
                        <view class="c-form-main-name fv" wx:if="{{$comForm$item.type=='date'}}">
                            <text class="f26">{{$comForm$item.name}}</text>
                            <input disabled bindtap="$comForm$showData" data-com-index="{{index}}" data-wpyshowdata-a="{{$comForm$index}}" name="{{$comForm$item.name}}" placeholder="请选择时间" placeholderClass="c99 f24" type="text" value="{{$comForm$vm.pickTime[$comForm$index]}}"></input>
                            <view class="t-pick" wx:if="{{$comForm$vm.showPicker[$comForm$index]}}">
                                <view catchtap="$comForm$comPicker$Timeshow" class="t-pick__bg" data-com-index="{{index}}-{{$comForm$index}}" data-wpytimeshow-a="0"></view>
                                <view class="t-pick__main">
                                    <view class="t-btn f fj">
                                        <view catchtap="$comForm$comPicker$Timeshow" data-com-index="{{index}}-{{$comForm$index}}" data-wpytimeshow-a="0" data-wpytimeshow-b="{{$comForm$index}}">取消</view>
                                        <view catchtap="$comForm$comPicker$Timeshow" data-com-index="{{index}}-{{$comForm$index}}" data-wpytimeshow-a="1" data-wpytimeshow-b="{{$comForm$index}}" data-wpytimeshow-c="{{$comForm$vm.time}}">确定</view>
                                    </view>
                                    <picker-view bindchange="$comForm$comPicker$bindChange" class="pick" data-com-index="{{index}}-{{$comForm$index}}" data-wpybindchange-a="{{$comForm$vm.time}}" indicatorStyle="height: 50px;" value="{{$comForm$vm.time.value}}">
                                        <picker-view-column>
                                            <view wx:for="{{$comForm$vm.time.years}}" wx:for-index="$comForm$comPicker$index" wx:for-item="$comForm$comPicker$item" wx:key="$comForm$comPicker$key">{{$comForm$comPicker$item}}年</view>
                                        </picker-view-column>
                                        <picker-view-column>
                                            <view wx:for="{{$comForm$vm.time.months}}" wx:for-index="$comForm$comPicker$index" wx:for-item="$comForm$comPicker$item" wx:key="$comForm$comPicker$key">{{$comForm$comPicker$item}}月</view>
                                        </picker-view-column>
                                        <picker-view-column>
                                            <view wx:for="{{$comForm$vm.time.days}}" wx:for-index="$comForm$comPicker$index" wx:for-item="$comForm$comPicker$item" wx:key="$comForm$comPicker$key">{{$comForm$comPicker$item}}日</view>
                                        </picker-view-column>
                                        <picker-view-column>
                                            <view wx:for="{{$comForm$vm.time.hours}}" wx:for-index="$comForm$comPicker$index" wx:for-item="$comForm$comPicker$item" wx:key="$comForm$comPicker$key">{{$comForm$comPicker$item}}时</view>
                                        </picker-view-column>
                                        <picker-view-column>
                                            <view wx:for="{{$comForm$vm.time.mins}}" wx:for-index="$comForm$comPicker$index" wx:for-item="$comForm$comPicker$item" wx:key="$comForm$comPicker$key">{{$comForm$comPicker$item}}分</view>
                                        </picker-view-column>
                                    </picker-view>
                                </view>
                            </view>
                        </view>
                    </block>
                    <button class="skin_bg" formType="submit">提交</button>
                </view>
            </view>
        </form>
        <view class="com-bottomnav tc f bg-w" wx:if="{{item.type=='bottomnav'}}">
            <view bindtap="$comBottomnav$bottomGo" class="f1 f fv fc-h fc {{$comBottomnav$item.url==currentPageIndex?'skin_txt':'b8b8'}}" data-com-index="{{index}}" data-wpybottomgo-a="{{$comBottomnav$item}}" data-wpybottomgo-b="{{$comBottomnav$index}}" wx:for="{{item.navlist}}" wx:for-index="$comBottomnav$index" wx:for-item="$comBottomnav$item" wx:key="$comBottomnav$key">
                <image class="bo-img" mode="aspectFill" src="{{$comBottomnav$item.img}}" wx:if="{{$comBottomnav$item.img}}"></image>
                <view class="dzicon {{$comBottomnav$item.icon}} f40" wx:else></view>
                <text class="f20" style="font-weight:bold">{{$comBottomnav$item.name}}</text>
            </view>
            <view bindtap="$comBottomnav$meGo" class="f1 f fv fc-h fc b8b8" data-com-index="{{index}}">
                <image class="bo-img" mode="aspectFill" src="{{vm_com_bottom.img}}" wx:if="{{vm_com_bottom.img}}"></image>
                <view class="dzicon {{vm_com_bottom.icon||'icon-personal4-33'}} f40" wx:else></view>
                <text class="f20" style="font-weight:bold">{{vm_com_bottom.name||'我的'}}</text>
            </view>
        </view>
        <view class="c-news" wx:if="{{item.type=='news'}}">
            <view class="component-title f fc fc-h" wx:if="{{item.title}}">
                <text class="component-title-left dzicon icon-_xiegan skin_txt"></text>{{item.title}}<text class="component-title-right dzicon icon-_xiegan skin_txt"></text>
            </view>
            <view class="c-newsLst">
                <view class="c-news-big" wx:if="{{item.liststyle=='4'}}">
                    <view bindtap="$comNews$goNews" class="c-news-big-main" data-com-index="{{index}}" data-wpygonews-a="{{$comNews$item.id}}" wx:for="{{vm_com_news[currentPageIndex+'_'+index].list}}" wx:for-index="$comNews$index" wx:for-item="$comNews$item" wx:key="$comNews$key">
                        <image mode="widthFix" src="{{$comNews$item.img||'http://j.vzan.cc/miniapp/img/enterprise/blank_150x150.png'}}"></image>
                        <view class="f30 line line1">{{$comNews$item.title}}</view>
                        <view class="f26 c666 line line2 mt20" wx:if="{{$comNews$item.description}}">{{$comNews$item.description}}</view>
                        <view class="f fj w-100 f24 c999" style="margin-top:30rpx;">
                            <text>{{$comNews$item.addtime}}</text>
                            <text>{{$comNews$item.VirtualPV+$comNews$item.PV||0}}浏览</text>
                        </view>
                    </view>
                </view>
                <view class="c-news-small" wx:if="{{item.liststyle=='5'}}">
                    <view bindtap="$comNews$goNews" class="c-news-small-main f" data-com-index="{{index}}" data-wpygonews-a="{{$comNews$item.id}}" wx:for="{{vm_com_news[currentPageIndex+'_'+index].list}}" wx:for-index="$comNews$index" wx:for-item="$comNews$item" wx:key="$comNews$key">
                        <view class="f1 f fv fj">
                            <view class="f fv">
                                <view class="line line1">{{$comNews$item.title}}</view>
                                <view class="f26 c666 line line2 mt20" wx:if="{{$comNews$item.description}}">{{$comNews$item.description}}</view>
                            </view>
                            <view class="f fj w-100 f24 c999">
                                <text>{{$comNews$item.addtime}}</text>
                                <text>{{$comNews$item.VirtualPV+$comNews$item.PV||0}}浏览</text>
                            </view>
                        </view>
                        <image src="{{$comNews$item.img||'http://j.vzan.cc/miniapp/img/enterprise/blank_150x150.png'}}"></image>
                    </view>
                </view>
                <view class="c-news-txt" wx:if="{{item.liststyle=='1'}}">
                    <view bindtap="$comNews$goNews" class="c-news-txt-main f" data-com-index="{{index}}" data-wpygonews-a="{{$comNews$item.id}}" wx:for="{{vm_com_news[currentPageIndex+'_'+index].list}}" wx:for-index="$comNews$index" wx:for-item="$comNews$item" wx:key="$comNews$key">
                        <text class="dzicon icon-duihua skin_txt" style="margin-top:5rpx"></text>
                        <view class="f fv ml20 f1">
                            <text class="line line2 f30 f1">{{$comNews$item.title}}</text>
                            <view class="f fj w-100 f24 c999 mt20">
                                <text>{{$comNews$item.addtime}}</text>
                                <text>{{$comNews$item.VirtualPV+$comNews$item.PV||0}}浏览</text>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
            <view bindtap="$comNews$goMore" class="c-newsLst-btn" data-com-index="{{index}}" data-wpygomore-a="{{item.typeid}}" data-wpygomore-b="1" wx:if="{{vm_com_news[currentPageIndex+'_'+index].showmore}}">查看全部</view>
        </view>
