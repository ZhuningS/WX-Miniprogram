.c-subLst {
    width: 100%;
    padding: 30rpx;
}

.c-subLst .c-subLst-main {
    width: 100%;
    background-color: #fff;
    padding: 30rpx;
    border-radius: 15rpx;
    margin-bottom: 50rpx;
}

.c-subLst .c-subLst-main .c-subLst-main-status {
    font-size: 24rpx;
    color: #666;
    margin-bottom: 30rpx;
}

.c-subLst .c-subLst-main .c-subLst-main-status>text:first-child {
    display: inline-block;
    width: 15rpx;
    height: 15rpx;
    border-radius: 50%;
    margin-right: 10rpx;
}

.c-subLst .c-subLst-main .c-subLst-main-same {
    margin-bottom: 20rpx;
    font-size: 26rpx;
}

.c-subLst .c-subLst-main .c-subLst-main-same>text:first-child {
    color: #999;
    width: 130rpx;
    display: inline-block;
}

.c-subLst .c-subLst-main .c-subLst-main-same>text:last-child {
    color: #333;
}