@import "../../__wuBaseWxss__/9.wxss";

@import "../../__wuBaseWxss__/8.wxss";

.c-subform {
    width: 100%;
}

.c-subform .c-subform-main {
    width: 100%;
    background: #fff;
    padding: 0 30rpx;
}

.c-subform .c-subform-main .c-subform-input {
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
    border-bottom: 1px solid #eee;
}

.c-subform .c-subform-main .c-subform-input input {
    text-align: right;
    font-size: 34rpx;
    height: 120rpx;
    line-height: 120rpx;
}

.c-subform .c-subform-main .c-subform-title {
    font-size: 34rpx;
    width: 140rpx;
}

.c-subform .c-subform-btn {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    font-size: 30rpx;
    line-height: 100rpx;
    background: #FAFAFA;
    z-index: 10;
}

.c-subform .c-subform-btn .c-subform-sub {
    height: 100rpx;
}