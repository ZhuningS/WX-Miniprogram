.g-pro {
    width: 100%;
    height: 300rpx;
    background-color: #fff;
    padding: 20rpx;
    margin-top: 20rpx;
}

.g-pro .g-img {
    width: 246rpx;
    height: 259rpx;
    margin-right: 20rpx;
}

.g-pro .g-num {
    width: 80rpx;
    height: 40rpx;
    line-height: 35rpx;
    text-align: center;
    font-size: 22rpx;
    border: 1px solid #ffb049;
    color: #ffb049;
    border-radius: 18rpx;
}

.cffb {
    color: #ffb049;
}

.groupUserList {
    text-align: center;
    padding: 20rpx;
}

.groupUserList .user-photo {
    position: relative;
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
}

.groupUserList .user-photo image {
    width: 100rpx;
    height: 100rpx;
    vertical-align: middle;
    border-radius: 100%;
    background-color: #ccc;
}

.groupUserList .tuanzhang {
    width: 64rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    background-color: #FF9A37;
    color: #fff;
    font-size: 20rpx;
    position: absolute;
    bottom: -10rpx;
    left: 50%;
    transform: translateX(-50%);
    border-radius: 10rpx;
}

.groupUserList .user-name {
    margin-top: 20rpx;
    color: #333;
}

.groupUserList .groupStateWatermark {
    position: absolute;
    right: -40rpx;
    top: -60rpx;
    border: 2rpx solid #B1B1B1;
    box-sizing: border-box;
    padding: 6rpx;
    border-radius: 100%;
}

.groupUserList .groupStateWatermark .innerWrap {
    background-color: #B1B1B1;
    width: 120rpx;
    height: 120rpx;
    text-align: center;
    border-radius: 100%;
    justify-content: center;
}

.groupUserList .groupStateWatermark text {
    font-size: 24rpx;
    color: #F3F3F3;
    display: inline-block;
    width: 56rpx;
    -webkit-transform: rotate(-20deg);
}

.groupUserList .group-tip {
    color: #FF9A37;
    font-size: 32rpx;
    margin-top: 40rpx;
    font-weight: bold;
}

.groupUserList .fromTheEnd {
    justify-content: center;
}

.groupUserList .fromTheEnd text {
    background-color: #fff;
    position: relative;
    z-index: 2;
    padding: 0;
}

.groupUserList .bg-vc-line {
    position: absolute;
    left: 0;
    width: 100%;
    height: 1rpx;
    background-color: #d9d9d9;
    top: 50%;
    transform: translateY(-50%);
}

.groupUserList .group-state {
    padding: 20rpx 45rpx;
    background-color: #ededed;
    display: inline-block;
    margin: 0 auto;
    border-radius: 6rpx;
    line-height: 1;
}

.g-des {
    margin-top: 20rpx;
    background: #fff;
}

.g-des .g-title {
    border-bottom: 1px solid #eee;
}

.g-des .g-title .g-item {
    height: 100rpx;
    line-height: 100rpx;
    color: #999;
    text-align: center;
}

.g-des .g-del {
    padding: 40rpx 20rpx;
}

.g-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    background: #fff;
}

.g-bottom .g-home {
    padding: 0 20rpx;
    text-align: center;
}

.g-bottom .g-btn {
    height: 100rpx;
    color: #fff;
    justify-content: center;
    text-align: center;
    background-color: #ff9a37;
}

.g-bottom .g-end {
    width: 100%;
    height: 100rpx;
    background: #cbc5bc;
    text-align: center;
    font-size: 32rpx;
    color: #fff;
    line-height: 100rpx;
}