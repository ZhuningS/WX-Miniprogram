@import "../../__wuBaseWxss__/12.wxss";

@import "../../__wuBaseWxss__/8.wxss";

@import "../../__wuBaseWxss__/2.wxss";

.c-news {
    width: 100%;
}

.c-news .c-news-video {
    width: 100%;
    height: 450rpx;
}

.c-news .c-news-video .c-news-video-cover,.c-news .c-news-video video {
    width: 100%;
    height: 450rpx;
}

.c-news .c-news-video .c-news-video-btn {
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -50rpx;
    margin-top: -50rpx;
}

.c-news .c-news-audio {
    width: 100%;
    padding: 30rpx 0;
}

.c-news .c-news-audio>image {
    width: 500rpx;
    height: 500rpx;
}

.c-news .c-news-audio .c-news-audio-progress {
    width: 500rpx;
    font-size: 20rpx;
    margin-top: 40rpx;
}

.c-news .c-news-audio .c-news-audio-progress>slider {
    width: 364rpx;
    margin: 0 10rpx;
}

.c-news .c-news-audio .c-news-audio-btn {
    width: 88rpx;
    height: 88rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -44rpx;
    margin-top: -50rpx;
}

.c-news .c-news-main {
    width: 100%;
    padding: 30rpx;
    background-color: #fff;
}

.c-news .c-news-rich {
    padding: 20rpx 0;
    background-color: #fff;
    margin-top: 20rpx;
}

.c-news .c-news-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    z-index: 1;
}

.c-news .c-news-bottom .c-news-bottom-btn {
    line-height: 100rpx;
    text-align: center;
    font-size: 36rpx;
}

.c-news .c-news-bottom .c-good-btn-icon {
    color: #777;
    width: 125rpx;
    height: 100rpx;
    font-size: 24rpx;
}

.c-news .c-icon-other-main {
    position: fixed;
    bottom: 10%;
    right: 30rpx;
    z-index: 87;
}