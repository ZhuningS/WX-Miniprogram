page {
    background: #fff;
}

.vip {
    width: 100%;
}

.vip .logo {
    font-size: 24rpx;
    color: #999;
    margin-top: 30rpx;
}

.vip .super {
    font-size: 50rpx;
    width: 340rpx;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    border-radius: 40rpx 4rpx 40rpx 4rpx;
    margin: 30rpx auto 20rpx auto;
}

.vip .super-next {
    width: 220rpx;
    height: 60rpx;
    background: #666;
    text-align: center;
    line-height: 60rpx;
    border-radius: 30rpx 2rpx 30rpx 2rpx;
    margin: 30rpx auto 20rpx auto;
}

.vip .show-line {
    width: 1rpx;
    height: 100rpx;
}

.vip .line {
    width: 690rpx;
    height: 1rpx;
    background: #eee;
    margin: 50rpx auto;
}

.vip .form {
    width: 100%;
}

.vip .form .form-title {
    font-size: 24rpx;
    color: #999;
    margin-bottom: 30rpx;
}

.vip .form .main {
    font-size: 24rpx;
    width: 497rpx;
}

.vip .form .main .main-title {
    width: 100%;
    height: 62rpx;
    background: #e2e2e2;
    border-top-left-radius: 5rpx;
    border-top-right-radius: 5rpx;
    line-height: 62rpx;
}

.vip .form .main .main-title>text {
    display: inline-block;
    width: 50%;
    padding-left: 30rpx;
    border-right: 1px solid #d9d8d9;
}

.vip .form .main .main-title>text:last-child {
    border-right: none;
}

.vip .form .main .main-lst {
    width: 100%;
    border-left: 1px solid #e2e2e2;
    border-right: 1px solid #e2e2e2;
    border-bottom: 1px solid #e2e2e2;
    border-bottom-left-radius: 5rpx;
    border-bottom-right-radius: 5rpx;
}

.vip .form .main .main-lst .main-item {
    width: 100%;
    height: 62rpx;
}

.vip .form .main .main-lst .main-item>text {
    display: inline-block;
    width: 50%;
    height: 62rpx;
    line-height: 62rpx;
    padding-left: 30rpx;
    border-top: 1px solid #d9d8d9;
    border-right: 1px solid #d9d8d9;
}

.vip .form .main .main-lst .main-item>text:last-child {
    border-right: none;
}