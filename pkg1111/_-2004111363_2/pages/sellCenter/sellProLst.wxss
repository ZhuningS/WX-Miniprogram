.sell-header {
    width: 100%;
    height: 200rpx;
    background: #fff;
    padding: 32rpx 20rpx 0 20rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
}

.sell-search {
    width: 100%;
    height: 68rpx;
    background: #f6f6f6;
    border-radius: 6rpx;
    padding: 0 20rpx;
}

.sell-input {
    width: 100%;
    height: 68rpx;
    margin-left: 10rpx;
}

.sell-classify {
    width: 100%;
    margin-top: 35rpx;
}

.pb30 {
    padding-bottom: 28rpx;
}

.sell-list {
    width: 100%;
    padding: 20rpx;
}

.sell-item {
    width: 345rpx;
    background: #fff;
    margin-bottom: 20rpx;
}

.sell-item>image {
    width: 345rpx;
    height: 345rpx;
}

.sell-price {
    font-size: 38rpx;
    color: #f20033;
}