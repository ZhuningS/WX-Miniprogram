    margin-bottom: 10rpx;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-logo>text {
    font-size: 24rpx;
    color: #666;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-title {
    margin-top: 90rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: #444;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-title>text {
    color: #999;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-good {
    width: 100%;
    height: 260rpx;
    background-color: #f3f3f3;
