    padding: 0 30rpx;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-good>image {
    width: 200rpx;
    height: 200rpx;
    border-radius: 5rpx;
    margin-right: 30rpx;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-good .c-bn-cutdown {
    font-size: 24rpx;
    color: #444;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-good .c-bn-cutdown .c-btn-time:first-child {
    margin-left: 0!important;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-good .c-bn-cutdown .c-btn-time {
    width: 45rpx;
    padding: 5rpx 0;
    text-align: center;
    border: 1px solid #d9d8d9;
    border-radius: 5rpx;
    margin-right: 8rpx;
    margin-left: 8rpx;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank {
    width: 100%;
    margin-top: 90rpx;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank progress {
    width: 100%;
    border-radius: 100px;
    overflow: hidden;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank>text {
    position: absolute;
    bottom: -50rpx;
    font-size: 24rpx;
    color: #a1a2a3;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank .c-bn-rank-right {
    right: 0;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank .c-bn-rank-left {
    left: 0;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank .c-bn-rank-toast {
    position: absolute;
    top: -65rpx;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank .c-bn-rank-toast text:first-child {
    border: 8px solid transparent;
    border-top: 8px solid #ffecdd;
    width: 0;
    height: 0;
    position: absolute;
    bottom: -30rpx;
    left: 40%;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-rank .c-bn-rank-toast text:last-child {
    border-radius: 5rpx;
    padding: 5rpx 10rpx;
    color: #ff6f30;
    font-size: 24rpx;
    background-color: #ffecdd;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-btn {
    width: 100%;
    height: 80rpx;
    margin-top: 80rpx;
}

.c-bn .c-bn-main .c-bn-main-top .c-bn-btn>text,.c-bn .c-bn-main .c-bn-main-top .c-bn-btn button {
    font-size: 30rpx;
    width: 100%;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    display: inline-block;
    border-radius: 50rpx;
    background-image: -webkit-gradient(linear,left top,left bottom,from(#FF7437),to(#FF7437));
    -webkit-animation-timing-function: ease-in-out;
    -webkit-animation-name: breathe;
    -webkit-animation-duration: .5s;
    -webkit-animation-iteration-count: infinite;
    -webkit-animation-direction: alternate;
}

@-webkit-keyframes breathe {
    0% {
        transform: scale(.9);
    }

    100% {
        transform: scale(1);
    }
}

.c-bn .c-bn-main .c-bn-main-bottom {
