<view class="page__bd page__bd_spacing">
    <view class="kind-list">
        <view class="kind-list__item" wx:for="{{list}}">
            <view bindtap="kindToggle" class="weui-flex kind-list__item-hd {{item.open?'kind-list__item-hd_show':''}}" id="{{index}}">
                <view class="weui-flex__item">{{item.question}}</view>
                <image class="kind-list__img" src="https://api.cnmmsc.org/apitc/img/image/huise@2x.png"></image>
            </view>
            <view class="kind-list__item-bd {{item.open?'kind-list__item-bd_show':''}}">
                <view class="weui-cells {{item.open?'weui-cells_show':''}}">
                    <view class="weui-cell__bd" style="word-wrap:break-word;">{{item.answer}}</view>
                </view>
            </view>
        </view>
    </view>
</view>
<view class="loading rel">
    技术支持@移动营销服务中心
  </view>
