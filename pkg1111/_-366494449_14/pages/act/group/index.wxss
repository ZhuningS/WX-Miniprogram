.panel-cm {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
}

.panel-mask {
    background-color: rgba(0,0,0,0.6);
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    opacity: 0;
}

.panel-content {
    background: #fff;
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    z-index: 2;
    padding-bottom: 92rpx;
    transform: translate(0,100%);
}

.panel-g-item {
    background-color: #fff;
    padding: 10rpx 0 30rpx 40rpx;
    border-bottom: 1rpx solid #f5f5f5;
    display: flex;
}

.panel-cm .g-item .price {
    color: #ef5b6b;
    font-size: 18px;
}

.panel-cm .img-wrap {
    width: 20%;
    height: 100%;
}

.panel-cm .img-wrap:before {
    content: '';
    display: block;
    margin-top: 100%;
}

.panel-cm .g-mid {
    margin: 0 0 0 30rpx;
    padding: 20rpx 0;
    vertical-align: bottom;
    flex: 1;
}

.panel-cm .g-title {
    font-size: 26rpx;
    color: #444;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
    margin-bottom: 10rpx;
}

.g-title text {
    font-size: 28rpx;
}

.panel-close {
    -webki-background-size: contain;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center center;
    background-image: url('data:image/png;base64,');
    width: 40rpx;
    height: 40rpx;
    padding: 40rpx 0;
}

.panel-cm .cm-content {
    background-color: #fff;
    padding: 0 4%;
    max-height: 500rpx;
    overflow-y: auto;
}

.panel-cm .cm-content view {
    position: relative;
    top: 0;
    left: 0;
}

.panel-cm .cm-amount {
    background: #fff;
    font-size: 32rpx;
    border-top: 1rpx solid #eee;
    padding: 20rpx 20rpx 10rpx;
}

.cm-amount .disabled {
    background-color: #eee;
}

.panel-cm .amount-bar {
    background-color: #fff;
}

.cm-foot {
    position: fixed;
    background-color: #fff;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 10010;
}

.cm-foot .btn {
    border-radius: 8rpx;
    border: 0;
    display: block;
    font-size: 32rpx;
    height: 80rpx;
    text-align: center;
    width: 100%;
}

.panel-cm.add-phone .panel-content {
    background-color: #f0eff4;
}

.panel-cm.add-phone .cm-content {
    max-height: 170px;
}

.panel-cm.add-phone .cm-amount {
    background-color: #fafafa;
}

.goodsImg {
    position: relative;
    top: -20rpx;
    top: 20rpx;
    width: 150rpx;
    height: 150rpx;
    vertical-align: bottom;
    border-radius: 4rpx;
}

.stock-num {
    float: left;
    line-height: 80rpx;
}

.stock-num text {
    font-size: 26rpx;
    color: #666;
}

.stock-num .limitNum {
    margin-left: 30rpx;
    font-size: 24rpx;
    color: #fab705;
}

.my-bnt {
    float: right;
}

.my-bnt .bnt {
    display: inline-block;
    height: 70rpx;
    line-height: 70rpx;
    width: 70rpx;
    border: 2rpx solid #eee;
    text-align: center;
    vertical-align: top;
}

.bnt text {
    font-size: 16px;
}

.my-bnt .ipt {
    display: inline-block;
    height: 70rpx;
    line-height: 70rpx;
    width: 70rpx;
    border-top: 2rpx solid #eee;
    border-bottom: 2rpx solid #eee;
    text-align: center;
    vertical-align: top;
    font-size: 28rpx;
}

.dib {
    display: inline-block;
    vertical-align: top;
}

.sku-btn {
    border: 1rpx solid #ddd;
    color: #666;
    display: inline-block;
    box-sizing: border-box;
    min-width: 160rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 50rpx;
    margin-bottom: 20rpx;
    margin-right: 20rpx;
    padding: 0 4px;
    position: relative;
}

.sku-btn text {
    font-size: 28rpx;
}

.sku-btn-selected {
    border: 1rpx solid #ef5b6b;
    color: #ef5b6b;
}

.sku-btn-selected:after {
}

.sku-label {
    padding: 20rpx 0;
}

.sku-label text {
    font-size: 28rpx;
}

.phone-panel {
    padding: 0 0 30rpx;
    position: relative;
    margin-top: 40rpx;
}

.phone-title {
    color: #666;
    font-size: 30rpx;
    line-height: 1;
    margin-bottom: 20rpx;
    padding: 0 3.125%;
}

.phone-title text {
    font-size: 16px;
}

.phone-content {
    background-color: #fff;
    padding: 0 3.125%;
}

.input-wrap {
    border-bottom: 1rpx solid #d7d7d7;
    margin-bottom: 0;
    display: flex;
}

.input-wrap .lab {
    display: block;
    width: 106rpx;
    padding: 0;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 32rpx;
}

.input-wrap .lab text {
    font-size: 16px;
    color: #666;
}

.input-wrap input {
    display: block;
    flex: 1;
    padding-left: 10rpx;
    text-align: left;
    font-size: 16px;
    background-color: transparent;
    height: 88rpx;
    line-height: 88rpx;
}

.yzm {
    display: flex;
    margin-bottom: 40rpx;
}

.yzm .input-wrap {
    margin-right: 0;
    border-right: 1rpx solid #d7d7d7;
    flex: 1;
}

.yzm .button {
    color: #68b42a;
    text-align: center;
}

.yzm .button-wrap {
    color: #68b42a;
    padding: 28rpx 0;
    line-height: 1;
    border-bottom: 1rpx solid #d7d7d7;
    margin: 0;
    text-align: center;
    width: 220rpx;
}

.yzm .yzm-text {
    color: #999;
    width: 220rpx;
    padding: 28rpx 0;
    border-bottom: 1rpx solid #d7d7d7;
    text-align: center;
    line-height: 1;
}

.group_goods {
    display: flex;
    box-shadow: 0rpx 2rpx 10rpx rgba(7,17,27,.1);
    border-radius: 8rpx;
    background: #fff;
    margin: 30rpx;
    padding: 20rpx;
}

.group_goods .goods_info {
    position: relative;
    margin-left: 20rpx;
}

.group_goods .goods_info .name {
    font-size: 32rpx;
    color: #333;
    margin-bottom: 10rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    line-height: 42rpx;
    word-break: break-all;
}

.group_goods .goods_info .num {
    width: 80rpx;
    line-height: 32rpx;
    color: #ffa04c;
    border-radius: 4rpx;
    font-size: 24rpx;
    border: 1px solid #ffa04c;
    text-align: center;
    padding: 2rpx 4rpx;
}

.group_goods .goods_info .price {
    position: absolute;
    font-size: 36rpx;
    color: #ff4751;
    bottom: -10rpx;
}

.group_goods .goods_info .price .old {
    font-size: 28rpx;
    color: #aaa;
    margin-left: 20rpx;
    text-decoration: line-through;
    font-weight: 300;
}

.group_status {
    display: flex;
    align-items: baseline;
    background: #fff;
    padding: 48rpx 30rpx 0;
}

.group_status .text1 {
    color: #333;
    font-size: 32rpx;
    margin: 0 auto;
}

.group_status .t2 {
    color: #666;
    font-size: 24rpx;
    flex: 1;
}

.group_status .t3 {
    display: flex;
    align-items: center;
    color: #666;
    font-size: 24rpx;
}

.group_team {
    background: #fff;
    padding: 10rpx 30rpx;
}

.group_btn_join {
    margin: 30rpx 0 0;
    background: #ff4751;
    border-radius: 10rpx;
    line-height: 88rpx;
    font-size: 36rpx;
    color: #fff;
    text-align: center;
}

.team_group {
    background: #fff;
    margin: 30rpx 30rpx 20rpx;
    overflow: hidden;
    text-align: center;
}

.team_group .avatar {
    display: inline-block;
    position: relative;
    width: 64rpx;
    height: 64rpx;
    margin: 6rpx;
}

.icon_tuanzhang {
    position: absolute;
    right: 0;
    height: 32rpx;
    width: 32rpx;
    bottom: 0;
}

.team_group .avatar .icon_avatar {
    width: 64rpx;
    height: 64rpx;
    border-radius: 100%;
}

.group_goods .goods_img {
    width: 236rpx;
    height: 236rpx;
}

.group_goods .goods_img image {
    width: 236rpx;
    height: 236rpx;
    border-radius: 8rpx;
}

.group_play {
    background: #fff;
    margin-top: 20rpx;
}

.group_play .title {
    border-bottom: 1px solid #eee;
    font-size: 28rpx;
    padding: 20rpx 30rpx;
}

.group_play .body {
    padding: 60rpx 30rpx;
    text-align: center;
    line-height: 50rpx;
}

.timeDown view {
    width: 50rpx;
    display: block;
    text-align: center;
    font-size: 26rpx;
    letter-spacing: 2rpx;
    background: #3a3b4d;
    border-radius: 4rpx;
    padding: 0 6rpx;
    margin: 0 6rpx;
    color: #fff;
    line-height: 38rpx;
}

.body .text text {
    width: 230rpx;
    display: inline-block;
    color: #333;
    font-size: 24rpx;
}

.card_shop image {
    width: 38rpx;
    height: 38rpx;
    border-radius: 100%;
    margin-right: 10rpx;
}

.card_shop {
    margin: 20rpx 30rpx;
    font-size: 24rpx;
    color: #aaa;
    display: flex;
    align-items: center;
}

.btn-order {
    border: 1px solid #ff4751;
    border-radius: 6rpx;
    font-size: 24rpx;
    padding: 4rpx 20rpx;
    line-height: 40rpx;
    color: #ff4751;
    margin: 0 10rpx;
}

.btn-order2 {
    background: #ff4751;
    border-radius: 6rpx;
    font-size: 24rpx;
    padding: 4rpx 20rpx;
    color: #fff;
    margin: 0 10rpx;
    line-height: 40rpx;
}

.button-bar {
    display: flex;
    align-items: center;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 92rpx;
    background: #18a80b;
    z-index: 999;
}

.btn-yaoqing {
    width: 150rpx;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    position: relative;
    height: 92rpx;
    background-color: #fea526!important;
}

.btn-baoming {
    color: #fff;
    background: #18a80b;
    font-size: 36rpx;
    flex: 1;
    line-height: 92rpx;
    text-align: center;
    padding: 0;
    margin: 0;
    border-radius: 0;
}

.btn-baoming text {
    font-size: 36rpx;
}

.form-mod {
    margin-top: 20rpx;
    background: #fff;
}

.form-controller {
    display: flex;
    padding: 30rpx 30rpx;
    border-bottom: 1px solid #eee;
    align-items: center;
}

.form-controller .label {
    font-size: 28rpx;
    margin-right: 20rpx;
    width: 130rpx;
    color: #666;
    text-align: left;
    white-space: nowrap;
}

.form-controller .input {
    flex: 1;
}

.form-controller .input input {
    font-size: 28rpx;
}

.tips {
    position: absolute;
    top: -80rpx;
    left: 40rpx;
    background: rgba(0,0,0,.7);
    font-size: 28rpx;
    color: #fff;
    padding: 10rpx;
    border-radius: 8rpx;
    white-space: nowrap;
    font-weight: 300;
}

.tips view::after {
    content: "";
    position: absolute;
    left: 20rpx;
    bottom: -14rpx;
    display: inline-block;
    width: 14rpx;
    height: 6rpx;
    background-image: url("http://img.weixin.sundayer.cn/card/jiao@3x.png");
    background-size: 100%;
    z-index: 5;
}

.group_btn_join {
    margin: 30rpx 0;
    background: #fea526;
    border-radius: 10rpx;
    line-height: 88rpx;
    font-size: 36rpx;
    color: #fff;
    text-align: center;
}