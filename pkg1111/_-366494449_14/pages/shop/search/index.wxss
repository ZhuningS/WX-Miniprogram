page {
}

.search_mod {
    display: flex;
    background: #fff;
    padding: 20rpx;
    align-items: center;
}

.search_mod .text {
    font-size: 28rpx;
    color: #333;
}

.search_bar {
    display: flex;
    color: #bbb;
    border-radius: 50rpx;
    height: 60rpx;
    line-height: 60rpx;
    background: #f5f5f5;
    flex: 1;
    margin-right: 30rpx;
    width: 490rpx;
    padding: 0 6rpx 0 30rpx;
    align-items: center;
}

.search_bar image {
    width: 26rpx;
    height: 26rpx;
    margin-right: 10rpx;
}

.search_bar input {
    flex: 1;
    font-size: 28rpx;
    color: #333;
}

.history {
    min-height: 200rpx;
    padding-left: 30rpx;
    font-size: 28rpx;
    background: #fff;
}

.history .last {
    position: relative;
    padding: 30rpx 0;
    display: flex;
    align-items: center;
}

.history .last text {
    font-size: 28rpx;
}

.history .last .delete {
    position: absolute;
    right: 30rpx;
    width: 28rpx;
    height: 28rpx;
}

.history_list .item {
    border-bottom: 1px solid #eee;
    color: #666;
    padding: 20rpx 0;
}

.goodsList_style02 {
    overflow: hidden;
    padding: 30rpx 30rpx 0;
}

.goods {
    width: 50%;
    float: left;
}

.goods_wrap {
    position: relative;
    background-color: #fff;
    margin: 14rpx;
}

.goods_style02 {
    margin: 0 0 30rpx 0;
}

.goods_wrap_style02 {
    display: flex;
    position: relative;
    background-color: #fff;
}

.goods__imgWrap_style02 {
    position: relative;
    height: 250rpx;
    width: 250rpx;
}

button.goods_wrap::after {
    border-radius: 0;
    border: none;
}

.goods__imgWrap {
    position: relative;
    height: 332rpx;
}

.goods__imgWrap:after {
    content: ' ';
    display: block;
    margin-top: 100%;
}

.goods__img {
    width: 100%;
    height: 332rpx;
    border-radius: 8rpx 8rpx 0 0;
}

.goods__img_style02 {
    height: 250rpx;
    border-radius: 8rpx;
}

.goodsInfoWrap {
    padding: 16rpx 20rpx;
    border-radius: 0 0 8rpx 8rpx;
    border: 1px solid #eee;
    height: 180rpx;
}

.goodsInfoWrap_style02 {
    position: relative;
    padding: 0 20rpx;
    flex: 1;
}

.goods__name {
    font-size: 30rpx;
    color: #333;
    overflow: hidden;
    line-height: 42rpx;
    margin-bottom: 10rpx;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.goods__name_style02 {
    font-size: 32rpx;
    color: #333;
    overflow: hidden;
    line-height: 42rpx;
    margin-bottom: 20rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.shortitle_style02 {
    font-size: 24rpx;
    color: #a3a3a3;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    margin-top: 10rpx;
}

.shortitle {
    font-size: 24rpx;
    color: #a3a3a3;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.goods__price {
    position: absolute;
    bottom: 72rpx;
    display: flex;
    align-items: baseline;
    color: #ff4751;
    font-size: 36rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-top: 20rpx;
    margin-left: -2rpx;
}

.goods__price .priceshow {
    font-size: 24rpx;
    color: #aaa;
    margin-left: 20rpx;
    text-decoration: line-through;
}

.btn-buy {
    position: absolute;
    bottom: 0;
    line-height: 48rpx;
    background: #ff4751;
    color: #fff;
    border-radius: 4rpx;
    text-align: center;
    font-size: 24rpx;
    width: 160rpx;
}

.saled {
    position: absolute;
    bottom: 0;
    right: 0;
    color: #aaa;
    font-size: 24rpx;
}

.good-mod {
    background: #fff;
    padding-top: 20rpx;
    padding-bottom: 10rpx;
}

.noResult {
    text-align: center;
    color: #aaa;
    padding: 30rpx 0;
    font-size: 28rpx;
    margin-top: 180rpx;
    font-weight: 300;
}

.label_discount {
    width: 112rpx;
    font-size: 22rpx;
    color: #ff4751;
    border-radius: 4rpx;
    text-align: center;
    border: 1px solid #ff4751;
    padding: 2rpx 0;
}