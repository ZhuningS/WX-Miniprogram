page {
    background: url('https://img.weixin.sundayer.cn/card/BG2@3x.png') no-repeat;
    background-size: cover;
}

.goodsInfo {
    margin: 172rpx auto 0;
    width: 690rpx;
    height: 390rpx;
    background-color: #fff;
    border-radius: 8rpx;
    position: relative;
    padding-top: 64rpx;
}

.avatar {
    position: absolute;
    top: -64rpx;
    left: 0;
    right: 0;
    margin: auto;
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    box-sizing: border-box;
    border: 4rpx solid #fff;
}

.username {
    font-size: 32rpx;
    color: #000;
    text-align: center;
}

.Logan {
    font-size: 28rpx;
    color: #ff854f;
    text-align: center;
}

.order-info {
    background: #f2f2f2;
    padding: 16rpx 10rpx;
    margin: 32rpx auto;
    width: 646rpx;
    height: 160rpx;
    border-radius: 8rpx;
    position: relative;
    display: flex;
}

.order-info .bg-img {
    width: 140rpx;
    height: 140rpx;
    margin-right: 24rpx;
}

.order-info .order-r {
    flex: 1;
}

.order-r text {
    font-size: 28rpx;
    color: #666;
    overflow: hidden;
    line-height: 42rpx;
    margin-bottom: 20rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.order-r .label-text {
    margin-bottom: 20rpx;
    line-height: 30rpx;
}

.timeDown {
    display: flex;
    align-items: center;
    color: #333;
    font-size: 20rpx;
    font-weight: bold;
}

.timeDown view {
    width: 40rpx;
    height: 34rpx;
    display: block;
    font-weight: normal;
    text-align: center;
    font-size: 22rpx;
    letter-spacing: 2rpx;
    background: #3a3b4d;
    border-radius: 6rpx;
    margin: 0 6rpx;
    color: #fff;
    line-height: 34rpx;
}

.countdown {
    display: flex;
    align-items: center;
    margin: 32rpx 0;
    justify-content: center;
    color: #3a3b4d;
    font-size: 22rpx;
}

.bargain-process {
    width: 690rpx;
    height: 312rpx;
    overflow: hidden;
    border-radius: 8rpx;
    background-color: rgba(0,0,0,.2);
    margin: 0 auto;
}

.bargain-process>.success {
    font-size: 28rpx;
    color: #fff;
    height: 44rpx;
    text-align: center;
    margin: 30rpx auto;
}

.popup,.popup1 {
    text-align: center;
    line-height: 40rpx;
    padding: 10rpx;
    background: #ffe361;
    height: 60rpx;
    color: #e54c49;
    font-size: 30rpx;
    border-radius: 8rpx;
    position: absolute;
    top: -92rpx;
    box-shadow: #f7ac4f 0rpx 4rpx;
}

.popup {
    left: 44%;
}

.popup1 {
    right: 20%;
}

.popup>view,.popup1>view {
    width: 0;
    height: 0;
    border-style: solid;
    position: absolute;
}

.popup>view {
    left: 0;
    bottom: -16rpx;
    border-width: 20rpx 20rpx 0 0;
    border-color: #ffe361 transparent transparent transparent;
}

.popup1>view {
    right: 0;
    border-width: 0 20rpx 20rpx 0;
    border-color: transparent #ffe361 transparent transparent;
    bottom: -14rpx;
}

.popup>text,.popup1>text {
    font-size: 36rpx;
}

.process-wrap {
    width: 640rpx;
    margin: 12rpx auto 0;
    position: relative;
}

.process-wrap>image {
    position: absolute;
    width: 60rpx;
    height: 47rpx;
    right: 50%;
    top: 50%;
    transform: translateY(-50%);
}

.process-bar {
    width: 640rpx;
    height: 16rpx;
    overflow: hidden;
    border-radius: 12rpx;
    background: url('https://img.weixin.sundayer.cn/image/pngprocess-bar.png') no-repeat;
    background-size: 100%;
}

.process {
    width: 50%;
    height: 100%;
    background: url('https://img.weixin.sundayer.cn/image/pngprocess-cont.png') repeat-x;
    background-size: 56rpx 16rpx;
}

.normal-deep {
    width: 640rpx;
    margin: 16rpx auto 18rpx;
    display: flex;
    justify-content: space-between;
    align-content: center;
    color: #ff9e99;
    font-size: 22rpx;
}

.invite-btn {
    width: 640rpx;
    height: 84rpx;
    margin: 0 auto;
    font-size: 32rpx;
    color: #e54c49;
    background-color: #ffe361;
    border-radius: 8rpx;
    box-shadow: #f7ac4f 0 4rpx;
    text-align: center;
    line-height: 84rpx;
    animation: change .8s infinite;
}

.myCut {
    width: 640rpx;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
}

.myCut>view,.myCut>button {
    width: 310rpx;
    height: 84rpx;
    font-size: 32rpx;
    border-radius: 8rpx;
    text-align: center;
    line-height: 84rpx;
}

.pay {
    color: #fff331;
    background-color: #ff911b;
    box-shadow: #df5e09 0 4rpx;
}

.invite {
    color: #e54c49;
    background-color: #ffe361;
    box-shadow: #f7ac4f 0 4rpx;
    padding: 0;
    animation: change .8s infinite;
}

.herolist {
    width: 690rpx;
    max-height: 820rpx;
    overflow: hidden;
    border-radius: 8rpx;
    background-color: rgba(0,0,0,.2);
    margin: 32rpx auto 40rpx;
}

.list-tit {
    height: 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 40rpx 0 32rpx;
}

.list-tit>image,.recommend-tit>image {
    width: 80rpx;
    height: 4rpx;
}

.list-tit>text,.recommend-tit>text {
    font-size: 32rpx;
    color: #fff;
    margin: 0 24rpx;
}

.list-item {
    height: 128rpx;
    width: 640rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 auto;
}

.heroInfo,.achievement {
    display: flex;
    justify-content: center;
    align-items: center;
}

.heroInfo>image {
    width: 88rpx;
    height: 88rpx;
    box-sizing: border-box;
    border-radius: 50%;
    margin-right: 24rpx;
    border: 4rpx solid rgba(255,255,255,.4);
}

.heroInfo .name {
    font-size: 28rpx;
    color: #fff;
}

.heroInfo .tip {
    font-size: 28rpx;
    color: #ff9e99;
}

.achievement>image {
    width: 40rpx;
    height: 34rpx;
    margin-right: 12rpx;
}

.achievement {
    font-size: 28rpx;
    color: #ffe361;
}

.moreitem {
    margin: 6rpx auto 32rpx;
    font-size: 28rpx;
    color: #ff9e99;
    display: flex;
    justify-content: center;
    align-items: center;
}

.moreitem>image {
    width: 22rpx;
    height: 16rpx;
    margin-left: 8rpx;
}

.recommend {
    width: 100%;
}

.recommend-tit {
    height: 80rpx;
    background-color: #b63a3a;
    display: flex;
    justify-content: center;
    align-items: center;
}

.good-mod {
    background-color: #9f3232;
    width: 100%;
    padding-bottom: 10rpx;
}

.goodsList {
    overflow: hidden;
    margin: 0 20rpx;
}

.goods {
    width: 50%;
    float: left;
}

.goods_wrap {
    position: relative;
    background-color: #fff;
    margin: 14rpx;
    border-radius: 8rpx;
}

.goods_style02 {
    margin: 0 0 40rpx 0;
}

.goods_wrap_style02 {
    display: flex;
    position: relative;
    background-color: #fff;
}

.goods__imgWrap_style02 {
    position: relative;
    height: 260rpx;
    width: 260rpx;
}

button.goods_wrap::after {
    border-radius: 0;
    border: none;
}

.goods__imgWrap {
    position: relative;
    height: 332rpx;
    border: 1px solid #eee;
    border-bottom: 0;
}

.goods__imgWrap:after {
    content: ' ';
    display: block;
    margin-top: 100%;
}

.goods__img {
    width: 100%;
    height: 332rpx;
}

.goodsInfoWrap1 {
    padding: 16rpx 20rpx;
    border-radius: 0 0 8rpx 8rpx;
    border: 1px solid #eee;
    height: 180rpx;
}

.goods__name {
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
    line-height: 42rpx;
    margin-bottom: 10rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.shortitle {
    font-size: 24rpx;
    color: #a3a3a3;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    height: 34rpx;
}

.goods__price {
    display: flex;
    align-items: baseline;
    color: #ff4751;
    font-size: 36rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-top: 20rpx;
    margin-left: -2rpx;
}

.goods__price__unit {
    font-size: 26rpx;
}

.kan-success {
    width: 640rpx;
    height: 608rpx;
    border-radius: 16rpx;
    position: fixed;
    z-index: 4;
    top: 344rpx;
    left: 0;
    right: 0;
    margin: auto;
    background-color: #fff;
}

.kan-success>image {
    width: 100%;
    height: 320rpx;
}

.kan-price {
    font-size: 36rpx;
    color: #333;
    text-align: center;
    margin: 48rpx auto;
}

.kan-price>text {
    font-size: 36rpx;
    color: #e54c49;
}

.btn-wrap {
    display: flex;
    justify-content: center;
}

.btn-wrap>view,.btn-wrap>button {
    width: 268rpx;
    height: 84rpx;
    border-radius: 8rpx;
    font-size: 32rpx;
    line-height: 84rpx;
    box-sizing: border-box;
    text-align: center;
}

.certain {
    border: 2rpx solid #f7ac4f;
    color: #ff8e02;
}

.join {
    border: 2rpx solid #f7ac4f;
    color: #e54c49;
    background-color: #ffe361;
    box-shadow: #f7ac4f 0 4rpx;
    padding: 0;
}

.close {
    width: 48rpx;
    height: 88rpx;
    position: fixed;
    top: 228rpx;
    right: 60rpx;
    z-index: 9999;
}

.rule-btn {
    width: 128rpx;
    height: 48rpx;
    position: fixed;
    top: 32rpx;
    right: 0;
    font-size: 24rpx;
    text-align: center;
    line-height: 48rpx;
    border-radius: 24rpx 0 0 24rpx;
    color: #333;
    background-color: rgba(255,255,255,.4);
}

.rules {
    width: 640rpx;
    height: 658rpx;
    padding: 0 40rpx;
    border-radius: 16rpx;
    position: fixed;
    z-index: 4;
    top: 314rpx;
    left: 0;
    right: 0;
    margin: auto;
    background-color: #fff;
}

.rules .rules-tit {
    font-size: 36rpx;
    color: #333;
    text-align: center;
    line-height: 48rpx;
    margin: 24rpx 0 20rpx;
}

.rules>view {
    margin-bottom: 20rpx;
    font-size: 26rpx;
    line-height: 34rpx;
    color: #888;
}

.overwrap {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,.6);
}

@-webkit-keyframes change {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(.96);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes change {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(.96);
    }

    100% {
        transform: scale(1);
    }
}