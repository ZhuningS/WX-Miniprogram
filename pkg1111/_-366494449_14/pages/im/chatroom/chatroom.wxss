}

.self>.msg::after {
    content: "";
    position: absolute;
    right: -16rpx;
    left: auto!important;
    top: 20rpx;
    display: inline-block;
    width: 0;
    height: 0;
    border-style: solid;
    border-color: #ebebeb #ebebeb #ebebeb #b2e281!important;
    border-width: 10rpx;
    transform: rotate(0deg);
    z-index: 5;
}

.main .msg::after {
    content: "";
    position: absolute;
    left: -16rpx;
    top: 20rpx;
    display: inline-block;
    width: 0;
    height: 0;
    border-style: solid;
    border-color: #ebebeb #fff #ebebeb #ebebeb;
    border-width: 10rpx;
    transform: rotate(0deg);
    z-index: 5;
}