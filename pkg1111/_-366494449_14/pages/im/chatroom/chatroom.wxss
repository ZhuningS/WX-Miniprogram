@-webkit-keyframes bofang {
    25% {
        background-position: -46rpx 0;
    }

    50% {
        background-position: -96rpx 0;
    }

    75% {
        background-position: -142rpx 0;
    }

    100% {
        background-position: 0 0;
    }
}

@keyframes bofang {
    25% {
        background-position: -46rpx 0;
    }

    50% {
        background-position: -96rpx 0;
    }

    75% {
        background-position: -142rpx 0;
    }

    100% {
        background-position: 0 0;
    }
}

.self .laba {
