page {
    background: #dd2450;
}

.coupon {
    display: inline-block;
    width: 685rpx;
    position: relative;
    margin-bottom: 20rpx;
    height: 215rpx;
}

.coupon__info {
    position: relative;
    padding: 48rpx 30rpx 20rpx;
    z-index: 99;
    color: #fff;
}

.coupon__main {
    color: #fff;
    width: 200rpx;
    text-align: center;
    display: flex;
    align-items: center;
    flex-direction: column;
    padding: 38rpx 0 36rpx;
}

.coupon__main.coupon__main_disabled {
    background-color: #d7d7d7;
}

.coupon__name {
    top: 22rpx;
    left: 20rpx;
    position: relative;
    color: #444;
    font-size: 26rpx;
}

.coupon__times {
    color: #444;
    font-size: 24rpx;
    line-height: 1;
    margin: 10rpx 0 0 0;
}

.coupon__time {
    text-align: left;
    margin-left: 40rpx;
}

.coupon__time__title {
    margin-right: 20rpx;
}

.coupon__price {
    display: flex;
    align-items: flex-end;
    justify-content: center;
    padding-bottom: 28rpx;
}

.coupon__price__unit {
    font-size: 24rpx;
}

.coupon__price__val {
    font-size: 60rpx;
    line-height: 1;
}

.coupon__get {
    font-size: 28rpx;
    color: #fff;
    padding: 20rpx 20rpx 0;
    line-height: 1;
    border-top: 1rpx solid rgba(255,255,255,.5);
}

.nodata {
    width: 100%;
    height: 100%;
    padding: 150px 0;
    text-align: center;
}

.nodata .icon {
    display: block;
    width: 100%;
    height: 100px;
    margin-bottom: 15px;
}

.nodata text {
    color: #666;
    line-height: 1;
    font-size: 30rpx;
    margin-bottom: 15px;
}

.nodata .btn-wrap {
    text-align: center;
    font-size: 30rpx;
    line-height: 1;
    margin-bottom: 15px;
}

.nodata .btnl {
    background-color: transparent;
    width: auto;
    line-height: 72rpx;
    padding: 0 37px;
    font-size: 30rpx;
}

.coupon_wrap {
    position: relative;
    z-index: 99;
    color: #444;
    overflow: hidden;
    margin-left: 40rpx;
    line-height: 70rpx;
}

.coupon_amount {
    font-size: 90rpx;
    float: left;
    font-weight: bold;
}

.coupon_mod {
    position: absolute;
    top: 30rpx;
    left: 50%;
    margin-left: -343rpx;
}

.listStylebp {
    margin: 10rpx;
    width: 47%;
}

.mod {
    position: relative;
    border-bottom: 1px solid #f3f3f3;
    padding: 22rpx 0 2rpx 0;
    background: #fff;
}

.mod_title {
    text-align: center;
    line-height: 32rpx;
}

.mod_title_line {
    height: 26rpx;
    position: relative;
    width: 230rpx;
    margin: 0 auto;
}

.mod_title_top {
    position: absolute;
    top: -16rpx;
    left: 50%;
    padding: 0 10rpx;
    width: 260rpx;
    margin-left: -130rpx;
    text-align: center;
}

.mod_title {
    position: absolute;
    bottom: 28rpx;
    left: 50%;
    width: 260rpx;
    margin-left: -130rpx;
}

.btn-lq {
    border-radius: 30rpx;
    border: 1px solid #999;
    display: inline-block;
    font-size: 24rpx;
    padding: 10rpx 20rpx;
    position: absolute;
    top: 75rpx;
    right: 53rpx;
    color: #444;
    z-index: 999;
    font-weight: 300;
}

.coupon.disabled .coupon__times,.coupon.disabled .coupon__name,.coupon.disabled .coupon_wrap {
    color: #999;
}