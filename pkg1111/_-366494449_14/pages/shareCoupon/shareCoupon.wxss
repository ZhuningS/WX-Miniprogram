.avatar {
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    margin: 0 auto;
    position: absolute;
    top: 176rpx;
    left: 0;
    right: 0;
    border: 2rpx solid #fff;
}

.origin {
    font-size: 24rpx;
    text-align: center;
    color: #666;
}

.coupon {
    background: #fff;
    width: 690rpx;
    height: 320rpx;
    margin: 64rpx auto 72rpx;
    position: relative;
    background: url('https://img.weixin.sundayer.cn/quan@3x.png') no-repeat 0 0/cover;
    box-shadow: 0 0 16rpx 4rpx #d9d9d9;
    border-radius: 10rpx;
}

.coupon .discount {
    font-size: 26rpx;
    color: #000;
    margin: 0 96rpx;
}

.discount text {
    color: #fb8115;
    font-size: 30rpx;
}

.top {
    font-size: 24rpx;
    color: #999;
    display: flex;
    align-items: center;
    height: 232rpx;
}

.coupon .tips {
    width: 100%;
    height: 88rpx;
    display: flex;
    align-items: center;
    color: #333;
    font-size: 24rpx;
}

.tips image {
    width: 56rpx;
    height: 56rpx;
    margin: 0 24rpx 0 30rpx;
    border-radius: 50%;
    border: 2rpx solid #ccc;
}

.accept-btn {
    width: 690rpx;
    height: 88rpx;
    background: #23a721;
    border-radius: 10rpx;
    line-height: 88rpx;
    text-align: center;
    margin: 0 auto;
    color: #fff;
    font-size: 36rpx;
}

.overwrap {
    background: rgba(0,0,0,.66);
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
}

.overwrap>view {
    width: 600rpx;
    height: 444rpx;
    border-radius: 16rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    background: #fff;
}

.overwrap image {
    display: block;
    width: 214rpx;
    height: 180rpx;
    margin: 32rpx auto;
}

.btnwrap {
    margin-top: 40rpx;
    display: flex;
    justify-content: center;
}

.btn {
    width: 236rpx;
    height: 68rpx;
    border-radius: 8rpx;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
    line-height: 68rpx;
    border: 2rpx solid #23a721;
    background-color: #23a721;
}

.void {
    background-color: #fff;
    color: #23a721;
    margin-right: 40rpx;
}