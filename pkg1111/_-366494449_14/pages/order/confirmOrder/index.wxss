.wrap {
    padding-bottom: 280rpx;
}

.wrap .copyright {
    bottom: 100rpx;
}

.clearfix:after {
    display: block;
    content: '';
    clear: both;
    visibility: hidden;
    width: 0;
    height: 0;
}

.left {
    float: left;
}

.right {
    float: right;
}

.navbarTit__item {
    line-height: 1.4;
}

@-webkit-keyframes gif {
    100% {
        transform: rotateZ(360deg);
    }
}

.icon-arrow {
    display: inline-block;
    -webki-background-size: contain;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center center;
    background-image: url('data:image/png;base64,');
    width: 7px;
    height: 12px;
}

.icon-addr {
    display: inline-block;
    -webki-background-size: contain;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center center;
    background-image: url('data:image/png;base64,');
    width: 42rpx;
    height: 42rpx;
}

.address {
    padding: 32rpx 10rpx 30rpx 70rpx;
    margin-top: 20rpx;
    position: relative;
    background-repeat: repeat-x;
    background-position: 0 bottom;
    background-size: 70rpx;
    background-color: #fff;
    background-image: url('data:image/jpeg;base64,');
}

.address .icon-addr {
    position: absolute;
    left: 3.125%;
    top: 50%;
    margin-top: -9px;
}

.address .addr-user {
    line-height: 1;
    margin: 0 0 16rpx 20rpx;
    color: #444;
    width: 90%;
}

.addr-user text {
    font-size: 28rpx;
}

.addr-addr {
    display: inline-block;
    width: 90%;
    line-height: 1.2;
    margin-left: 20rpx;
    word-break: break-all;
}

.addr-addr text {
    font-size: 28rpx;
    color: #444;
}

.address view view:last-child {
    margin-bottom: 0;
}

.wrap .no-addr {
    padding: 30rpx;
    margin-bottom: 20rpx;
    margin-top: 20rpx;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.no-addr text {
    font-size: 28rpx;
}

.address .user-name {
    margin-right: 30rpx;
}

.address .icon-arrow {
    position: absolute;
    top: 40%;
    right: 3.125%;
    width: 2%;
}

.contant {
    margin-top: 20rpx;
}

.cart-section {
    position: relative;
    background-color: #fff;
}

.cart-list {
    display: flex;
    flex-direction: row;
    border-bottom: 1px solid #eee;
    font-size: 0;
    position: relative;
    background-color: #fff;
    padding: 20rpx 30rpx 20rpx 0;
    margin-left: 30rpx;
}

.cart-list .img-wrap {
    margin-right: 20rpx;
}

.cart-list .goods-info {
    align-items: center;
    justify-content: center;
    color: #444;
    width: 100%;
}

.goods-info text {
    font-size: 30rpx;
}

.cart-list {
}

.cart-list .goods-title {
    display: block;
    line-height: 32rpx;
}

.cart-list .goods-attr {
    font-size: 24rpx;
    display: block;
    color: #999;
    margin: 0;
}

.cart-list .price-wrap {
    display: flex;
}

.price-wrap text {
    font-size: 28rpx;
}

.cart-list .price {
    font-size: 36rpx;
    color: #ff5c53;
    flex: 1;
    float: left;
}

.cart-list .couponprice {
    font-size: 22rpx;
    color: #ff5c53;
    float: left;
}

.cart-list .goods-amount {
    color: #666;
    float: right;
    margin-top: 10rpx;
}

.goods-trans-wrap {
    background-color: #fff;
}

.goods-trans {
    height: 90rpx;
    line-height: 88rpx;
    padding: 0 30rpx;
    background-color: #fff;
    border-bottom: 1px solid #eee;
}

.coupon-wrap,.member-wrap {
    height: 90rpx;
    line-height: 88rpx;
    padding: 0 30rpx;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 2rpx solid #eee;
}

.goods-trans_last {
    margin-bottom: 10px;
    border-bottom: 0;
}

.goods-trans text,.coupon-wrap text {
    font-size: 28rpx;
}

.goods-trans .price-total,.coupon-wrap .coupon-total {
    color: #ff5c53;
}

.coupon-wrap .coupon-total {
    display: flex;
    flex: 1;
    justify-content: flex-end;
    align-items: flex-end;
    color: #666;
    white-space: nowrap;
}

.coupon-name {
    width: 400rpx;
    height: 100%;
}

.member-wrap text:first-child {
    flex: 1;
    font-size: 28rpx;
    color: #333;
}

.member-wrap picker {
    margin-right: 20rpx;
    display: flex;
    flex: 2;
    justify-content: flex-end;
}

.member-wrap text:nth-child(2) {
}

.member-wrap .member_yue {
    margin-left: 20rpx;
}

.weui-input image {
    width: 36rpx;
    height: 36rpx;
    vertical-align: middle;
}

.idcard {
    margin-bottom: 20rpx;
}

.idcard .help {
    padding: 0 3.125%;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    color: #999;
    background-color: #fff;
    font-weight: 300;
    position: relative;
}

.idcard .help i {
    font-style: normal;
    font-size: 40rpx;
    margin-right: 2px;
    vertical-align: middle;
}

.idcard .help:before {
    content: '';
    display: block;
    border-color: transparent transparent #fafafa;
    border-style: solid;
    border-width: 4px;
    border-top-width: 0;
    position: absolute;
    top: -4px;
    left: 10%;
}

.help text {
    font-size: 24rpx;
}

.my-pic {
    width: 140rpx;
    height: 140rpx;
    border: 1px solid #f1f1f1;
    padding: 8rpx;
}

.btn-wrap {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 11;
}

.payagain-hint-wrap {
    line-height: 1.4;
    box-sizing: border-box;
    padding: 20rpx 30rpx;
    background: #fff;
}

.payagain-hint {
    font-size: 14px;
    color: #999;
}

.col-green {
    background-color: #00a671;
}

.goods-types {
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    height: 70rpx;
    line-height: 70rpx;
    border-bottom: 1rpx solid #eee;
    padding-left: 30rpx;
}

.goods-types text {
    font-size: 14px;
}

.goods-types .consume-type {
    color: #666;
}

.icon-arrow {
    width: 24rpx;
    height: 24rpx;
    vertical-align: middle;
}

.zan-dialog {
    position: relative;
    z-index: 999;
}

.zan-dialog__mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
    background: rgba(0,0,0,0.7);
    display: none;
}

.zan-dialog__container {
    position: fixed;
    bottom: 0;
    width: 750rpx;
    background: white;
    transform: translateY(150%);
    transition: all 0.4s ease;
    z-index: 11;
    min-height: 500rpx;
}

.zan-dialog--show .zan-dialog__container {
    transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
    display: block;
}

.zan-dialog__container scroll-view {
    max-height: 300px;
}

.choose-coupon {
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    height: 66px;
    line-height: 37px;
    padding: 14px 0;
    margin-left: 30rpx;
    border-bottom: 1px solid #eee;
}

.choose-coupon text {
    font-size: 32rpx;
    color: #515f64;
}

.coupon-list {
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    height: 66px;
    line-height: 37px;
    padding: 14px 0;
    margin: 0 30rpx;
    border-bottom: 1px solid #eee;
}

.coupon-list__money {
    height: 80rpx;
    line-height: 80rpx;
    color: #dd2450;
    margin-right: 20rpx;
    font-size: 34rpx;
    width: 90rpx;
    white-space: nowrap;
}

.coupon-list__money text {
    font-size: 60rpx;
    color: #dd2450;
    margin-left: 10rpx;
}

.coupon-list__con {
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    margin-left: 40rpx;
    padding-left: 40rpx;
    border-left: 1px solid #eee;
}

.coupon-list__con .coupon-name {
    color: #999;
    line-height: 40rpx;
    font-size: 26rpx;
    margin-bottom: 5px;
}

.coupon-list__con .coupon-other {
    line-height: 22rpx;
    color: #999;
    font-size: 26rpx;
}

.coupon-list__icon {
    width: 18px;
    height: 18px;
}

.coupon-list__icon image {
    display: block;
    width: 18px;
    height: 18px;
}

.weui-check {
    position: absolute;
    left: -9999px;
}

.price-calculation {
    padding: 10px 0;
    text-align: center;
    background-color: #fff;
    margin-bottom: 10px;
}

.price-calculation>text {
    display: block;
    padding: 0 30rpx;
    line-height: 18px;
    font-size: 12px;
    letter-spacing: 1px;
}

.price-calculation .price-calculation_discount {
    display: inline-block;
    line-height: 18px;
    font-size: 12px;
    letter-spacing: 1px;
}

.price-calculation_pay {
    color: #00a671;
}

.zan-loadmore {
    position: relative;
    width: 65%;
    margin: 21px auto;
    line-height: 20px;
    font-size: 14px;
    text-align: center;
    vertical-align: middle;
}

.zan-loadmore__tips {
    display: inline-block;
    vertical-align: middle;
    height: 20px;
    line-height: 20px;
}

.zan-loadmore--nodata,.zan-loadmore--nomore {
    border-top: 1rpx solid #e5e5e5;
    color: #999;
}

.zan-loadmore--nodata {
    max-height: 300px;
}

.zan-loadmore--nodata .zan-loadmore__tips {
    position: relative;
    top: -11px;
    background: #f9f9f9;
    padding: 0 6px;
}

.membershipList_fixed {
    display: hidden;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
}

.price-total {
    color: #666;
    font-size: 28rpx;
    float: right;
}

.weui-cell__bd {
    font-size: 30rpx;
    color: #444;
}

.weui-cells {
    margin-top: 20rpx;
}

.weui-cells:after {
    border-bottom: 0;
}

.weui-cells:before {
    border-top: 0;
}

.weui-cell:before {
    border-top: 1rpx solid #eee;
}

.btn-primary {
    background: #1aad19!important;
    height: 100rpx!important;
}

.buyer {
    border-bottom: 1px solid #eee;
    padding: 10rpx 28rpx;
    font-size: 24rpx;
    color: #999;
    display: flex;
    align-items: center;
}

.buyer image {
    width: 32rpx;
    height: 32rpx;
    border-radius: 100%;
    margin-right: 10rpx;
}