    right: 0;
    top: 0;
    width: 93rpx;
    height: 86rpx;
}

.coupon-item .discount {
    font-size: 35rpx;
    color: #fff;
    text-align: left;
    margin-bottom: 4rpx;
    height: 100rpx;
    line-height: 120rpx;
}

.discount text {
    position: relative;
    bottom: -2rpx;
    font-size: 60rpx;
}

.coupon-item view {
    font-size: 23rpx;
    color: #fffefe;
    padding: 0 30rpx;
}

.coupon-list {
    width: 100%;
    border-radius: 6rpx;
    padding-top: 12rpx;
    font-size: 24rpx;
