    line-height: 30rpx;
}

.good-mod .title .more {
    font-size: 24rpx;
    color: #999;
}

.renzheng {
    display: flex;
    align-items: center;
    font-size: 20rpx;
    padding: 4rpx 10rpx;
    border-radius: 4rpx;
    font-weight: 300;
    background: rgba(0,0,0,0.5);
    line-height: 30rpx;
    width: 128rpx;
    text-align: center;
    justify-items: center;
    white-space: nowrap;
}

.renzheng image {
    width: 22rpx;
    height: 22rpx;
