    padding: 0;
    font-size: 0;
    width: 48rpx;
    height: 48rpx;
    background: none;
    margin-right: 30rpx;
}

.buttonbar .button-item image {
    width: 48rpx;
    height: 48rpx;
}

.btn-buy {
    position: absolute;
    bottom: 2rpx;
    line-height: 48rpx;
    background: #ff4751;
    color: #fff;
    border-radius: 4rpx;
    text-align: center;
    font-size: 24rpx;
    width: 160rpx;
}

.button-item .cartBadge {
    display: inline-block;
    background-color: #e64340;
    color: #fff;
    text-align: center;
    font-size: 18rpx;
    height: 24rpx;
    line-height: 24rpx;
    width: 24rpx;
    position: absolute;
    top: -6rpx;
    right: -14rpx;
    border-radius: 50%;
    z-index: 99;
}

.good-mod {
    background: #fff;
    margin-top: 20rpx;
    padding-bottom: 10rpx;
}

.good-mod .title {
    display: flex;
    justify-content: space-between;
    font-weight: 500;
    align-items: center;
    position: relative;
    padding: 26rpx 30rpx;
    color: #333;
    font-size: 30rpx;
}

.good-mod .title .start {
    font-size: 32rpx;
    color: #333;
    border-left: 10rpx solid #333;
