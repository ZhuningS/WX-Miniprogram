    z-index: 999;
    top: 0;
    padding-right: 0;
}

.page .search {
    margin-bottom: -20rpx;
}

.mallname {
    font-size: 32rpx;
    color: #333;
    width: 490rpx;
    line-height: 96rpx;
}

.category {
    position: absolute;
    right: 0;
    background-color: #fff;
    box-shadow: -4rpx 0 22rpx rgba(150,145,145,0.575);
    top: 0;
    width: 48rpx;
    height: 100%;
}

.category image {
    margin: 26rpx auto;
    display: block;
    width: 28rpx;
    height: 28rpx;
}

.page .searchTip {
    margin-top: 20rpx;
}

.baoyou {
    font-size: 18rpx;
    text-align: center;
    margin-right: 10rpx;
    border-radius: 4rpx;
    color: #fea00b;
    border: 2rpx solid #fdbb50;
    padding: 0rpx 4rpx;
}

.yongquan {
    font-size: 18rpx;
    text-align: center;
    margin-right: 10rpx;
    border-radius: 4rpx;
    color: #ef7861;
    border: 2rpx solid #ef7861;
    padding: 0rpx 4rpx;
    width: 80rpx;
}

.queue {
    width: 48rpx;
    height: 48rpx;
}

.page__bd {
    position: relative;
    padding-bottom: 120rpx;
    top: 50rpx;
}

.page__bd__scroll {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}

.groupScroll {
    background-color: #fff;
    height: 81rpx;
    margin-bottom: 2rpx;
}

.groupList {
    padding: 0 10rpx;
    display: flex;
    flex-wrap: nowrap;
}

.groupItem {
    height: 80rpx;
    line-height: 80rpx;
    color: #333;
    font-size: 26rpx;
    margin: 0 30rpx;
    flex-shrink: 0;
}

.groupItem.active {
    color: #c00;
    border-bottom: 4rpx solid #c00;
}

.catPanel {
    width: 100%;
    background-color: #fff;
    border-bottom: 1px solid #eee;
}

.catPanel:after {
    content: '';
    display: block;
    clear: both;
}

.curCat {
    height: 85rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 33%;
    font-size: 14px;
    float: right;
}

.curCat__icon {
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: contain;
    width: 21rpx;
    height: 21rpx;
    margin-right: 18rpx;
}

.curCat__name {
    color: #000;
    max-width: 188rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
}

.catList {
    position: absolute;
    top: 85rpx;
    right: 0;
    width: 33%;
    background-color: #fff;
    box-shadow: 2px 2px 2px rgba(0,0,0,0.4);
    z-index: 10;
}

.catList__item {
    text-align: center;
    border-bottom: 1rpx solid #eee;
    color: #313131;
    display: block;
    height: 85rpx;
    line-height: 85rpx;
    padding: 0 24rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-size: 14px;
}

.goodsList {
    overflow: hidden;
    margin: 0 20rpx;
}

.goodsList_style02 {
    overflow: hidden;
    margin: 0 30rpx;
}

.goods {
    width: 50%;
    float: left;
}

.goods_wrap {
    position: relative;
    background-color: #fff;
    margin: 14rpx;
}

.goods_style02 {
    margin: 0 0 40rpx 0;
}

.goods_wrap_style02 {
    display: flex;
    position: relative;
    background-color: #fff;
}

.goods__imgWrap_style02 {
    position: relative;
    height: 260rpx;
    width: 260rpx;
}

button.goods_wrap::after {
    border-radius: 0;
    border: none;
}

.goods__imgWrap {
    position: relative;
    height: 332rpx;
    border: 1px solid #eee;
    border-bottom: 0;
    border-radius: 8rpx 8rpx 0 0;
}

.goods__imgWrap:after {
    content: ' ';
    display: block;
    margin-top: 100%;
}

.goods__img {
    width: 100%;
    height: 332rpx;
    border-radius: 8rpx 8rpx 0 0;
}

.goods__img_style02 {
    height: 250rpx;
    border-radius: 8rpx;
}

.yaoyue {
    width: 150rpx;
    height: 126rpx;
    position: fixed;
    right: 12rpx;
    bottom: 326rpx;
}

.goodsInfoWrap {
    padding: 16rpx 20rpx;
    border-radius: 0 0 8rpx 8rpx;
    border: 1px solid #eee;
    height: 180rpx;
}

.goodsInfoWrap1 {
    padding: 16rpx 20rpx;
    border-radius: 0 0 8rpx 8rpx;
    border: 1px solid #eee;
    height: 180rpx;
}

.goodsInfoWrap_style02 {
    position: relative;
    padding: 0 20rpx;
    flex: 1;
}

.goods__name {
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
    line-height: 42rpx;
    margin-bottom: 10rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.goods__name_style02 {
    font-size: 32rpx;
    color: #333;
    overflow: hidden;
    line-height: 42rpx;
    margin-bottom: 20rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.shortitle_style02 {
    font-size: 24rpx;
    color: #a3a3a3;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    margin-top: 10rpx;
}

.shortitle {
    font-size: 24rpx;
    color: #a3a3a3;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    height: 34rpx;
}

.goods__price {
    display: flex;
    align-items: baseline;
    color: #ff4751;
    font-size: 36rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-top: 20rpx;
    margin-left: -2rpx;
}

.goods__price_style02 {
    position: absolute;
    bottom: 70rpx;
    display: flex;
    align-items: baseline;
    color: #ff4751;
    font-size: 36rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-top: 20rpx;
    margin-left: -2rpx;
}

.goods__price_style02 .priceshow,.goods__price .priceshow {
    font-size: 24rpx;
    color: #aaa;
    margin-left: 20rpx;
    text-decoration: line-through;
    display: flex;
}

.goods__agio {
    flex: 1;
    font-size: 20rpx;
    color: #fff;
    background: #ef5b6b;
}

.gModList__salesVolume {
    flex: 1;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 22rpx;
    color: #999;
    text-align: right;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.gModList__salesVolume__item {
    display: inline-block;
    padding: 0 4rpx;
}

.goods__price__unit {
    font-size: 26rpx;
}

.goods__price__unit_2 {
    font-size: 24rpx;
}

.more-loading {
    display: flex;
    padding: 20rpx;
    justify-content: center;
}

.more-loading text {
    color: #666;
}

.searchTip {
    background-color: #fff;
    text-align: center;
    font-size: 28rpx;
    color: #888;
    padding: 200rpx 0 600rpx;
}

.shopHead {
    position: relative;
    background: #fff;
    height: 320rpx;
    overflow: hidden;
}

.shopname {
    position: absolute;
    display: flex;
    align-items: center;
    bottom: 20rpx;
    left: 30rpx;
    z-index: 12;
}

.shopname image {
    width: 88rpx;
    height: 88rpx;
    border-radius: 8rpx;
}

.shopname .text {
    font-size: 28rpx;
    color: #fff;
    margin-left: 20rpx;
}

.buttonbar {
    position: absolute;
