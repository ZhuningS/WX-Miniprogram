    border-radius: 8rpx;
    background-color: #fff;
    color: #eb5053;
    padding: 2rpx 0;
    font-size: 22rpx;
}

.title-bar {
    width: 100%;
    height: 0rpx;
    border-bottom: 1px solid #333;
}

.upper {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    padding: 0 30rpx;
    background: #fff;
}

.timeDown {
    display: flex;
    align-items: center;
    color: #333;
    font-size: 20rpx;
    font-weight: bold;
}

.timeDown view {
    width: 40rpx;
    height: 34rpx;
    display: block;
    font-weight: normal;
    text-align: center;
    font-size: 22rpx;
    letter-spacing: 2rpx;
    background: #3a3b4d;
    border-radius: 6rpx;
    margin: 0 6rpx;
    color: #fff;
    line-height: 34rpx;
}

.footer {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 98rpx;
    background-color: #23a721;
    color: #fff;
    font-size: 36rpx;
    text-align: center;
    line-height: 98rpx;
    border-radius: 0;
}