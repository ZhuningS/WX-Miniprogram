@import "../../../__wuBaseWxss__/0.wxss";

.wrap {
    padding-bottom: 200rpx;
}

.section {
    background-color: #fff;
    margin-bottom: 20rpx;
}

.user {
    position: relative;
    background: url("https://img.weixin.sundayer.cn/usercenterbg@3x.png") no-repeat;
    background-size: 100%;
    height: 280rpx;
    overflow: hidden;
}

.user__img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    background-color: #eee;
    border: 2rpx solid #fff;
}

.user__name text {
    font-size: 36rpx;
    color: #fff;
}

.barItem.barItem_orderTitle {
    height: 75rpx;
}

.contant {
    border-top: 2rpx solid #eee;
    border-bottom: 2rpx solid #eee;
    padding: 0 28rpx;
    background-color: #fff;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.order-title {
    height: 70rpx;
    line-height: 70rpx;
    overflow: hidden;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.order-title text {
    color: #666;
    font-size: 14px;
}

.myItemWrap {
    margin-top: 20rpx;
}

.cart-wrap,.addr-wrap,.coupon-wrap {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    background-color: #fff;
    padding: 10rpx 28rpx;
    border-bottom: 2rpx solid #eee;
}

.membership-wrap {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    background-color: #fff;
    padding: 10rpx 28rpx;
}

.cart {
    font-size: 30rpx;
    color: #666;
    position: relative;
}

.orderTypes {
    display: flex;
    padding: 20rpx 0;
}

.orderTypes__item {
    display: flex;
    flex: 1;
    justify-content: center;
    align-items: center;
}

.icon-wrap {
    position: relative;
    display: flex;
    flex: 1;
    margin: 18rpx 0 22rpx;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    overflow: inherit;
}

.icon-wrap image {
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
}

.order-types .icon {
    width: 44rpx;
    height: 44rpx;
    margin-bottom: 14rpx;
}

.order-status {
    padding-top: 20rpx;
    font-size: 24rpx;
    color: #666;
    line-height: 1;
}

.phone-content {
    background-color: #fff;
    border-top: 2rpx solid #eee;
    border-bottom: 2rpx solid #eee;
    padding: 20rpx 40rpx 60rpx;
    margin-top: 20rpx;
}

.input-wrap {
    border-bottom: 1rpx solid #d7d7d7;
    margin-bottom: 0;
    display: flex;
}

.input-wrap label {
    color: #666;
    display: block;
    width: 106rpx;
    padding: 0;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 32rpx;
}

.input-wrap input {
    display: block;
    flex: 1;
    padding-left: 10rpx;
    text-align: left;
    font-size: 32rpx;
    background-color: transparent;
    height: 88rpx;
    line-height: 88rpx;
}

.yzm {
    display: flex;
    margin-bottom: 40rpx;
}

.yzm .input-wrap {
    margin-right: 0;
    border-right: 1rpx solid #d7d7d7;
    flex: 1;
}

.yzm .button {
    padding: 28rpx 0;
    line-height: 1;
    border-bottom: 1rpx solid #d7d7d7;
    margin: 0;
    text-align: center;
    width: 220rpx;
}

.yzm .yzm-text {
    color: #999;
}

.coupon-num {
    position: absolute;
    right: 20px;
    top: 0;
    font-family: Arial,Helvetica,sans-serif;
    letter-spacing: 2px;
}

.membership-wrap_collect {
    margin-top: 20rpx;
}

.couponNum {
    font-size: 30rpx;
    color: #3d3d3d;
}

.barList {
    background-color: #fff;
    margin-bottom: 20rpx;
}

.barItem,button.nobutton.barItem {
    height: 88rpx;
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    border-bottom: 1rpx solid #eee;
}

.barItem.barItem_noMLeft {
    margin-left: 0;
}

.barItem.barItem_noBorder {
    border-bottom: 0;
}

.barList_sm .barItem {
    height: 70rpx;
}

.barItem:last-child {
    border-bottom: 0;
}

.barItem__icon {
    width: 32rpx;
    height: 32rpx;
    margin: 0 20rpx;
}

.barItem__info {
    flex: 1;
    font-size: 28rpx;
    display: flex;
    align-items: center;
}

.barItem__info__hd {
    width: 150rpx;
    margin-right: 60rpx;
    font-size: 28rpx;
    color: #333;
}

.barItem__info__bd {
    flex: 1;
}

.barItem__info__bd_right {
    text-align: right;
}

.barItem__arrowIcon {
    width: 20rpx;
    height: 20rpx;
    margin-left: 10rpx;
}

.barItem,button.nobutton.barItem {
    display: flex;
    align-items: center;
    margin-left: 30rpx;
    padding: 24rpx 30rpx 24rpx 0;
    line-height: 40rpx;
    border-bottom: 1rpx solid #f5f5f5;
}

#copyright {
    background: url(../../images/tabs/technical.png-do-not-use-local-path-./pages/my/home/index.wxss&313&16) no-repeat top center;
    background-size: 80px 60px;
}

.userinfo {
    border: 1px solid #fff;
    font-size: 22rpx;
    color: #fff;
    margin-top: 16rpx;
    padding: 0 8rpx;
    border-radius: 30rpx;
    background: transparent;
    line-height: 46rpx;
    width: 158rpx;
    margin-left: 0;
}

.user_info {
    margin-left: 30rpx;
}

.redpoint {
    position: absolute;
    display: flex;
    align-items: center;
    justify-content: center;
    top: -12rpx;
    right: -12rpx;
    width: 30rpx;
    height: 30rpx;
    border-radius: 100%;
    border: 1px solid #ff4751;
    color: #ff4751;
    text-align: center;
    font-size: 18rpx;
    line-height: 32rpx;
}

.version {
    color: #999;
    font-size: 22rpx;
    text-align: center;
    margin-top: 20px;
    position: absolute;
    bottom: 20rpx;
    left: 50%;
    margin-left: -80rpx;
    width: 160rpx;
    white-space: nowrap;
}

.scoreWrap {
    display: inline-block;
    padding: 0 20rpx;
    color: #ffe17d;
    font-size: 24rpx;
    font-weight: 400;
    background: rgba(00,00,00,.4);
    border-radius: 50rpx;
    line-height: 44rpx;
    text-align: center;
    margin-top: 10rpx;
    white-space: nowrap;
}

.memberlevel {
    width: 650rpx;
    height: 324rpx;
    position: absolute;
    top: 200rpx;
    left: 50%;
    margin-left: -325rpx;
    background-size: contain;
    background-position: 0 0;
    padding: 0 30rpx;
    border-radius: 20rpx;
}

.level1 {
    background-image: -webkit-linear-gradient(0deg,#F1D8A0,#D0A75D)!important;
}

.memberlabel {
    display: flex;
    align-items: center;
    padding: 0 10rpx;
    color: #A4B1C1;
    font-size: 24rpx;
    font-weight: 300;
    background: rgba(00,00,00,.4);
    border-radius: 20rpx;
    line-height: 40rpx;
    text-align: center;
    margin-top: 10rpx;
    white-space: nowrap;
}

.btn-ljlq {
    width: 140rpx;
    line-height: 48rpx;
    border-radius: 24rpx;
    border: 1px solid #fff;
    text-align: center;
    color: #fff;
    font-size: 24rpx;
    font-weight: 300;
}

.cardbg {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 16rpx;
}