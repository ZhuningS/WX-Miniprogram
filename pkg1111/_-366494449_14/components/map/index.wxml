<template name="map">
    <view style="padding:20rpx 20rpx 0; background: #fff;">
        <view style="padding: 20rpx 0; font-size: 28rpx;">地址: {{dataset.text}}</view>
        <map id="myMap" latitude="{{dataset.address.lat}}" longitude="{{dataset.address.lng}}" markers="{{markers}}" scale="16" style="width: 100%;height: 460rpx"></map>
    </view>
</template>
