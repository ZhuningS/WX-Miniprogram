.orderCard {
    padding: 0 30rpx;
    margin: 40rpx 0 20rpx;
    background: #fff;
    border-radius: 5px;
}

.orderCard__tit {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-bottom: 24rpx;
}

.orderCard__tag {
    width: 138rpx;
    height: 112rpx;
    background: url(data:image/png;base64,) no-repeat top center;
    background-size: 138rpx 112rpx;
    font-size: 60rpx;
    line-height: 112rpx;
    color: #fff;
    text-align: center;
}

.orderStatus {
    font-size: 30rpx;
    color: #ef5b6b;
}

.orderCard__item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    line-height: 48rpx;
    font-size: 28rpx;
    color: #666;
}

.order__item__tit {
    width: 140rpx;
}

.order__item__con {
    flex: 1;
    padding-left: 30rpx;
}

.shopName {
    flex: 1;
    padding-left: 36rpx;
    color: #b5a685;
}

.orderCard__price {
    padding-top: 20rpx;
    font-size: 48rpx;
    color: #ef5b6b;
    text-align: right;
    padding-bottom: 30rpx;
}

.orderCard__price__lab {
    display: inline-block;
    margin-right: 10rpx;
    font-size: 30rpx;
    color: #666;
    vertical-align: middle;
}

.orderDetail {
    padding: 0 30rpx;
    background: #fff;
    border-radius: 5px;
}

.salesCode {
    width: 100%;
    height: 110rpx;
    line-height: 110rpx;
    border-bottom: 1rpx solid #ddd;
    font-size: 36rpx;
    color: #666;
}

.goodsList2 {
    padding: 20rpx 0;
    border-bottom: 1rpx solid #ddd;
}

.goodsName {
    line-height: 50rpx;
    padding-top: 20rpx;
    font-size: 28rpx;
    color: #666;
}

.goodsList__item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    color: #666;
    line-height: 50rpx;
}

.goodsList__item__tit {
    width: 140rpx;
}

.goodsList__item__con {
    flex: 1;
    padding-left: 30rpx;
}

.orderDetail__footer {
    padding: 20rpx 0;
    font-size: 28rpx;
    color: #666;
}

.orderDetail__footer__item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    line-height: 50rpx;
}