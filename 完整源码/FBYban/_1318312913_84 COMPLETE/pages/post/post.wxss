}

.single-card-vote .options-ctn.pair .users-ctn {
    margin-top: 0;
    margin-left: 10rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.single-card-vote .options-ctn.pair .users-ctn .user {
