    display: flex;
    justify-content: center;
    align-items: center;
}

.single-card-vote .options-ctn .text-option .border .users-ctn .user .avatar {
    height: 23px;
    width: 23px;
}

.single-card-vote .options-ctn .text-option .border .users-ctn .user+.user {
    margin-left: -6px;
}

.single-card-vote .options-ctn .text-option .border .users-ctn .user.number {
    background-image: none;
    background-color: #FFFFFF;
    font-size: 10px;
}

.single-card-vote .options-ctn .text-option .border .users-ctn .percent {
    margin-left: 10rpx;
    font-size: 14px;
    line-height: 14px;
    height: 14px;
}

.single-card-vote .options-ctn .text-option+.text-option {
    margin-top: 33rpx;
}

.single-card-vote .options-ctn.pair {
    position: relative;
    width: 630rpx;
    transform: translateX(2rpx);
}

.single-card-vote .options-ctn.pair .status-ctn {
    position: relative;
    width: 100%;
}

.single-card-vote .options-ctn.pair .status-ctn .border {
    position: relative;
    height: 120rpx;
    width: 100%;
    border: 1rpx #CCCCCC solid;
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
}

.single-card-vote .options-ctn.pair .status-ctn .border .vote-area {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
