    height: 14px;
}

.single-card-vote .options-ctn.pair .text-option {
    display: block;
    width: 300rpx;
    padding: 22rpx 0;
    box-sizing: border-box;
    margin-top: 0;
    box-shadow: none;
    border: none;
}

.single-card-vote .options-ctn.pair .text-option .result {
    display: flex;
    align-items: center;
}

.single-card-vote .options-ctn.pair .text-option .result .users-ctn {
    margin-left: 0;
