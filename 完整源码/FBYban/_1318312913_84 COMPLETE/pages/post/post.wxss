}

.single-card-vote .options-ctn.pair .status-ctn .border .option {
    background-color: #93CFD8;
    position: relative;
    height: 100%;
    min-width: 1%;
    transition: all 1s;
}

.single-card-vote .options-ctn.pair .status-ctn .border .option+.option {
    background-color: #DE5956;
