}

.single-card-vote .options-ctn .text-option .border .text-ctn .selected {
    position: relative;
    z-index: -1;
    width: 12px;
    height: 12px;
    display: inline-block;
    justify-content: center;
    margin-left: 5rpx;
    transform: translateY(3rpx);
}

.single-card-vote .options-ctn .text-option .border .percentage {
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 1;
    height: 8rpx;
    transition: all 1s;
    background-color: #DE5956;
}

.single-card-vote .options-ctn .text-option .border .users-ctn {
    margin-top: 0;
    margin-left: 10rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.single-card-vote .options-ctn .text-option .border .users-ctn .user {
    box-shadow: 0 0 5px rgba(0,0,0,0.1),0 10px 10px rgba(0,0,0,0.05);
