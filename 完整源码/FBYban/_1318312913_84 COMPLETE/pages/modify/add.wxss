page {
    padding-bottom: 60px;
}

.section-name {
    color: rgba(0,0,0,0.2);
    font-size: 12px;
    line-height: 17px;
    margin: 30px 0 10px 60rpx;
}

.results {
    align-items: center;
    display: flex;
    margin-bottom: 30px;
    padding: 0 60rpx;
}

.results .icon {
    height: 40px;
    margin-right: 20px;
    width: 40px;
}

.results .title {
    flex: 1;
    font-size: 14px;
}