.vote-status-ctn {
    padding: 70rpx 60rpx;
    box-sizing: border-box;
}

.vote-status-ctn .label-ctn {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 100rpx;
}

.vote-status-ctn .label-ctn .meta {
    margin-top: 10rpx;
}

.vote-status-ctn .label-ctn .share {
    position: absolute;
    top: 0;
    right: 0;
}

.vote-status-ctn .user-list {
    margin-top: 60rpx;
}

.vote-status-ctn .user-list .user-ctn {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    height: 80rpx;
}

.vote-status-ctn .user-list .user-ctn+.user-ctn {
    margin-top: 40rpx;
}

.vote-status-ctn .user-list .user-ctn .avatar-ctn {
    position: relative;
    height: 80rpx;
    width: 80rpx;
}

.vote-status-ctn .user-list .user-ctn .avatar-ctn .avatar {
    width: 80rpx;
    height: 80rpx;
}

.vote-status-ctn .user-list .user-ctn .avatar-ctn .index {
    position: absolute;
    right: 0;
    bottom: 0;
    height: 40rpx;
    width: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #000;
    color: #FFF;
    font-size: 20rpx;
    border-radius: 50%;
}

.vote-status-ctn .user-list .user-ctn .detail {
    margin-left: 30rpx;
    height: 80rpx;
    display: flex;
    flex-flow: column;
    justify-content: center;
}

.vote-status-ctn .user-list .user-ctn .detail .name {
    font-size: 32rpx;
    line-height: 32rpx;
    height: 32rpx;
}