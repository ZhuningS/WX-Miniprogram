view {
    box-sizing: border-box;
}

.version-dialog-mask {
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 999;
    background-color: rgba(0,0,0,.3);
}

.version-dialog {
    position: absolute;
    padding: 30rpx;
    width: 80%;
    height: 700rpx;
    overflow: scroll;
    border-radius: 20rpx;
    background-color: white;
    font-size: 24rpx;
}

.version-dialog .title {
    font-size: 24rpx;
    color: #999;
}

.version-dialog .cnt {
    font-size: 24rpx;
    color: #000;
}

.btn-sure-wrap {
    position: absolute;
    right: 0;
    bottom: 0;
    padding: 40rpx 60rpx;
    font-size: 32rpx;
    color: #ff7800;
}

.version-items {
    padding-top: 20rpx;
}

.version-items .cnt {
    padding-left: 10rpx;
}